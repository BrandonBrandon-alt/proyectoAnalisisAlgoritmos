  booktitle={2023 4th International Conference on Artificial Intelligence, Robotics and Control (AIRC)}, 
  title={Generative Adversarial Networks as Weight Generators in Ensemble Classification}, 
  year={2023},
  volume={},
  number={},
  pages={1-5},
  abstract={Ensemble classifiers are constructed from various component classifiers by aggregating the results of the individual classifiers in some manner. While there are many ways to do this, one of the least computationally intensive is to use a voting ensemble, where the vote of each classifier is weighted by a number representing its performance. Choosing the appropriate weights is an involved process, that ranges from using trial and error to using sophisticated machine learning algorithms. In this paper, we proposed a novel technique that uses generative adversarial networks to customize the weights used for each individual input to the ensemble. Specifically, we propose to use conditional generative adversarial networks to generate the weights, with the input image acting as the label to the network and the appropriate weight being the output. We test the proposed system on an image classification task and show that the proposed method produces better results than using fixed weights by 2.43 percent.},
  keywords={Machine learning algorithms;Target recognition;Generative adversarial networks;Generators;Task analysis;Artificial intelligence;Robots;Conditional Generative Adversarial Networks;Ensemble classification;image recognition},
  doi={10.1109/AIRC57904.2023.10303257},
  ISSN={},
  month={May},}@INPROCEEDINGS{10692611,
  author={Cheng, Jiangmai and Jiang, Bo and Xu, Hong},
  booktitle={2024 5th International Conference on Artificial Intelligence and Electromechanical Automation (AIEA)}, 
  title={Reduce Deblurring Generative Networks Training Complexity with Encoder-Decoder Model}, 
  year={2024},
  volume={},
  number={},
  pages={1079-1083},
  abstract={We propose a deblurring method that integrates a combination of Generative Adversarial Networks (GANs) and encoder-decoder models. Our approach offers higher efficiency and reduced training complexity compared to generative deblurring models which utilizing GANs as the backbone network. In this approach, we employ the encoder as the feature extraction module, and then passes the features to the GANs for deblurring, followed by image reconstruction using the decoder as the last module. To ensure image fidelity, skip connections are introduced between the encoder and decoder. Meanwhile, we introduce a targeted training methodology designed to mitigate fluctuations in network parameters, thereby maximizing the efficacy of the model’s training velocity. We additionally conducted comparative evaluations based on the GoPro and Kohler datasets, contrasting our model against previously proposed algorithms to approve its exceptional performance in deblurring efficacy and efficiency.},
  keywords={Training;Fluctuations;Automation;Image synthesis;Feature extraction;Generative adversarial networks;Decoding;Complexity theory;Artificial intelligence;Image reconstruction;Generative Adversarial Networks;encoder-decoder models;Deblurring;Image Reconstruction;Skip Connection},
  doi={10.1109/AIEA62095.2024.10692611},
  ISSN={},
  month={June},}@INPROCEEDINGS{11189387,
  author={Althobaiti, Manal and Jun, Minhee},
  booktitle={2025 IEEE 2nd International Conference on Blockchain, Smart Healthcare and Emerging Technologies (SmartBlock4Health)}, 
