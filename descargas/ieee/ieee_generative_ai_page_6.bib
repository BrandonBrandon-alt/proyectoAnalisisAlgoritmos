  year={2024},
  volume={},
  number={},
  pages={1501-1507},
  abstract={In recent years, artificial intelligence (AI) has made significant advancements, but its application in precision-demanding fields like bartending remains limited. This paper presents an AI-based method for bartending that integrates computer vision and natural language processing (NLP) for precise ingredient identification and action generation. Using the YOLOv8 model, the system achieves real-time detection of bartending tools and ingredients, while Generative Adversarial Networks (GANs) are employed to generate customized bartending actions based on user preferences. This method enhances adaptability in dynamic bar environments, addressing gaps in current semi-automated bartending systems. The proposed system significantly improves operational efficiency, precision, and user satisfaction, showcasing the potential for advanced commercial AI applications in hospitality.},
  keywords={YOLO;Computer vision;Adaptation models;Computational modeling;Generative adversarial networks;Natural language processing;Real-time systems;Artificial intelligence;Bars;Artificial Intelligence;Bartending;Computer Vision;Natural Language Processing;YOLO;Generative Adversarial Networks;Precision Control},
  doi={10.1109/ICCC62609.2024.10942013},
  ISSN={2837-7109},
  month={Dec},}@INPROCEEDINGS{11027828,
  author={Shou, Yuancong and Zhang, Xiyuan and Wang, Chenan and Shen, Aoyi and Fan, Hao and Cai, Ruiyi and Yin, Yang and Zhou, Jiangping and Chai, Chunlei and Sun, Shouqian},
  booktitle={2024 17th International Symposium on Computational Intelligence and Design (ISCID)}, 
  title={Design Education in Human-AI Co-Creation: A Qualitative Analysis of Using Image Generative Models in Conceptual Design}, 
  year={2024},
  volume={},
  number={},
  pages={23-27},
  abstract={In the context of ongoing advancements in artificial intelligence technology, creative collaboration between humans and AI has become increasingly common. However, there remains a lack of comprehensive research on the long-term cooperation between creators and AI design tools. This study explores the evolution of the co-creation process between designers and AI through a nine-week innovative training workshop, inviting 30 designers to collaborate with the generative AI tool Midjourney. Four rounds of in-depth interviews were conducted with participants over two months, using the grounded theory method for qualitative analysis. Drawing from the findings, we have identified five core competencies necessary for co-creation with AI and provided suggestions for fostering creative collaboration with AI to guide design educational practice.},
  keywords={Training;Analytical models;Generative AI;Conferences;Computational modeling;Collaboration;Design tools;Interviews;Computational intelligence;human-AI co-creation;design education;grounded theory;image generative models},
  doi={10.1109/ISCID63852.2024.00014},
