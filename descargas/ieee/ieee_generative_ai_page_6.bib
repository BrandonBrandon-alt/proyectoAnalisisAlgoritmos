  author={He, Jiayi and Lai, Bingkun and Kang, Jiawen and Du, Hongyang and Nie, Jiangtian and Zhang, Tao and Yuan, Yanli and Zhang, Weiting and Niyato, Dusit and Jamalipour, Abbas},
  journal={IEEE Internet of Things Journal}, 
