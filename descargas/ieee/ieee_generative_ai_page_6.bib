  year={2024},
  volume={26},
  number={4},
  pages={97-103},
  abstract={This article explores the diverse applications of artificial intelligence (AI) across sectors in Africa and examines the roles of both local and foreign technology companies in its development. It also discusses challenges alongside strategies for advancing Africa’s AI ecosystem.},
  keywords={Ecosystems;Education;Entrepreneurship;Africa;Companies;Artificial intelligence;Economics;Developing countries;Urban planning;Rural areas;Generative AI;Market research;Market opportunities;Government policies},
  doi={10.1109/MITP.2024.3433729},
  ISSN={1941-045X},
  month={July},}@INPROCEEDINGS{10903726,
  author={Lorenza, Christina and Astuty, Eriana},
  booktitle={2024 6th International Conference on Cybernetics and Intelligent System (ICORIS)}, 
  title={AI-Driven Revolution: Effectiveness of Product Ads on Social Media Using Midjourney}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={The rapid development of artificial intelligence (AI) is inevitable and significantly impacts our lives. Although AI has many positive benefits, some research results show that it cannot fully replicate the complexity and subjectivity of human intelligence and emotions. The research objective is to assess AI's effectiveness in visualizing product advertisements using Midjourney on social media. The research used a quantitative approach with an online questionnaire as an instrument to collect data from the millennial generation and Gen Z in Bandung City from March to June 2024. The results of the descriptive analysis revealed that Gen Z is quite familiar with AI-based advertising and has good knowledge about artificial intelligence and its use. Consequently, Gen Z can easily understand AI-based product advertising messages, leading to increased brand awareness and recall. Overall, most Gen Z responded positively to the Midjourney application as a valuable tool for designers in developing advertising ideas on social media and expressed interest in viewing other product advertising visualizations using AI technology.},
  keywords={Industries;Visualization;Social networking (online);Instruments;Human intelligence;Urban areas;Millennials;Artificial intelligence;Advertising;Intelligent systems;Artificial Intelligence (AI);Social Media;Midjourney;Product Visualization;Advertisement;Millennials;Gen Z},
  doi={10.1109/ICORIS63540.2024.10903726},
  ISSN={},
  month={Nov},}@ARTICLE{10305704,
  author={Xu, Minrui and Niyato, Dusit and Zhang, Hongliang and Kang, Jiawen and Xiong, Zehui and Mao, Shiwen and Han, Zhu},
  journal={IEEE Vehicular Technology Magazine}, 
  title={Sparks of Generative Pretrained Transformers in Edge Intelligence for the Metaverse: Caching and Inference for Mobile Artificial Intelligence-Generated Content Services}, 
  year={2023},
  volume={18},
  number={4},
  pages={35-44},
  abstract={Aiming at achieving artificial general intelligence (AGI) for the metaverse, pretrained foundation models (PFMs), e.g., generative pretrained transformers (GPTs), can effectively provide various artificial intelligence (AI) services, such as autonomous driving, digital twins (DTs), and AI-generated content (AIGC) for extended reality (XR). With the advantages of low latency and privacy-preserving, serving PFMs of mobile AI services in edge intelligence is a viable solution for caching and executing PFMs on edge servers with limited computing resources and GPU memory. However, PFMs typically consist of billions of parameters that are computation- and memory-intensive for edge servers during loading and execution. In this article, we investigate edge PFM serving problems for mobile AIGC services of the metaverse. First, we introduce the fundamentals of PFMs and discuss their characteristic fine-tuning and inference methods in edge intelligence. Then, we propose a novel framework of joint model caching and inference for managing models and allocating resources to satisfy users’ requests efficiently. Furthermore, considering the in-context learning ability of PFMs, we propose a new metric to evaluate the freshness and relevance between examples in demonstrations and executing tasks, namely the Age of Context (AoC). Finally, we propose a least-context (LC) algorithm for managing cached models at edge servers by balancing the tradeoff among latency, energy consumption, and accuracy.},
  keywords={Task analysis;Servers;Metaverse;Artificial neural networks;Artificial intelligence;Adaptation models;Biological system modeling;Edge computing;Content management;Mobile communication},
  doi={10.1109/MVT.2023.3323757},
  ISSN={1556-6080},
  month={Dec},}@INPROCEEDINGS{10742828,
  author={Solanki, Ashish and Kang, Sandeep Singh and Singla, Sanjay},
  booktitle={2024 First International Conference on Technological Innovations and Advance Computing (TIACOMP)}, 
  title={Comprehensive Analysis on Image Generation using Quantum Generative Adversarial Network}, 
  year={2024},
  volume={},
  number={},
  pages={302-307},
  abstract={Images are a very useful form of information from medical diagnosis to scientific discovery. There are some areas where images are not enough but with the help of Generative Adversarial Networks (GANs) this problem is solved at some limit. Due to the success of classical generative adversarial networks, the quantum researcher created quantum version of GAN and also called as quantum generative adversarial network (QGANs) which works on quantum computers which use quantum mechanics techniques such as quantum entanglement, superposition, teleportation and many more. The successful implementation in real life of quantum computers are not easy because of limited quantum resources and access to quantum processors but instead of that there are different QGAN models for different purpose, such as drug discovery, finance, image generation and many more which shown good result as compare to their classical counterparts. This study is reviewed based on authors that proposed different QGAN models along with the techniques proposed on those papers for generating images, which datasets was considered, which parameters are considered, what their limitations and how to future research directions to overcome those problems.},
  keywords={Computers;Quantum computing;Quantum entanglement;Image synthesis;Noise;Quantum mechanics;Teleportation;Generative adversarial networks;Noise measurement;Integrated circuit modeling;Quantum Generative Adversarial Networks (QGANs);Classical Generative Adversarial Networks;Quantum Computing;Generative Adversarial Networks (GANs);Quantum Machine Learning (QML);Machine Learning (ML);Artificial Intelligence (AI);Image Generation},
  doi={10.1109/TIACOMP64125.2024.00057},
