  keywords={Deep learning;Image analysis;Generative adversarial networks;Medical diagnosis;Convolutional neural networks;Artificial intelligence;Medical diagnostic imaging},
  doi={10.1109/MITP.2021.3073665},
  ISSN={1941-045X},
  month={May},}@INPROCEEDINGS{11042848,
