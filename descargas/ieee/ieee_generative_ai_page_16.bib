  title={Artificial Intelligence (AI) Fundamentals for the Display Industry: A Review (August 2024)}, 
  year={2024},
  volume={1},
  number={},
  pages={204-213},
  abstract={Artificial intelligence (AI) has been implemented into the display industry in recent years. This paper introduces AI theories applied to the display industry, covering concepts of AI, machine learning, and deep learning. It reviews optimization techniques for developing machine learning models and explains various deep learning architectures and learning paradigms applicable to the display industry. Additionally, it introduces explainable AI techniques for model analysis. Practical application cases will be reviewed in a separate paper.},
  keywords={Training;Data models;Artificial intelligence;Predictive models;Machine learning;Deep learning;Industries;Optimization;Boosting;Reviews;Artificial intelligence;machine learning;deep learning;algorithms;displays},
  doi={10.1109/OJID.2024.3470711},
  ISSN={2836-211X },
  month={},}@INPROCEEDINGS{11059630,
  author={Oh, Eunseo and Ishtiaq, Mariam and Song, Junho and Won, Jongun},
  booktitle={2025 International Wireless Communications and Mobile Computing (IWCMC)}, 
  title={Transportation Failure Prediction Framework Using Explainable Artificial Intelligence and Quantum Algorithm}, 
  year={2025},
  volume={},
  number={},
  pages={1294-1299},
  abstract={By predicting and maintaining the failures in transportation systems such as trains and large vehicles, companies can improve operational efficiency. In particular, failures in brake-related components can lead to serious accidents, making it crucial to predict such failures. However, the data collected from transportation systems is often unstable and includes missing values due to factors such as sensor failures and battery discharge. This can lead to a lack of data for future analysis and incorrect results. Although various studies have attempted to address this issue, they have not considered the characteristics and uncertainties of the attributes that significantly impact failures. Therefore, this study applies explainable artificial intelligence to the preprocessed data to classify the attributes that significantly influence failures and uses a quantum algorithm-based stochastic differential equation to model the uncertainties by reflecting attribute characteristics in the process of handling missing values. By applying quantum algorithm to estimate the final missing data, this study enhances both the completeness of the data and the accuracy of product failure identification. To demonstrate the superiority of the proposed framework, missing values in a transportation dataset are estimated in two cases: air production unit data and air pressure system data related to the brake system in transportation. Subsequently, the effectiveness of the proposed approach, as compared with other algorithms, was demonstrated.},
  keywords={Wireless communication;Quantum algorithm;Uncertainty;Transportation;Stochastic processes;Production;Prediction algorithms;Mathematical models;Artificial intelligence;Mobile computing;transportation;failure prediction;missing value handling;explainable artificial intelligence;quantum algorithm},
  doi={10.1109/IWCMC65282.2025.11059630},
  ISSN={2376-6506},
  month={May},}@INPROCEEDINGS{11105811,
  author={Stewart, Callie and Gannod, Gerald C.},
  booktitle={2025 IEEE International Parallel and Distributed Processing Symposium Workshops (IPDPSW)}, 
  title={Experience using AI in MPI Test Suite Development: Implications for Educators}, 
  year={2025},
  volume={},
  number={},
  pages={688-691},
  abstract={The integration of artificial intelligence (AI) into software development has introduced new possibilities for automating complex coding tasks. This study investigates the role of AI, specifically GitHub Copilot, in generating Message Passing Interface (MPI) test suites and its implications for Parallel and Distributed Computing (PDC) education. Key findings highlight the effectiveness of AI-assisted development, the importance of prompt engineering, and the necessity of domain expertise. The study provides recommendations for educators on integrating AI tools into curriculum design while ensuring students develop critical programming skills. Future directions include enhancing AI capabilities for parallel computing and developing domain-specific models.},
  keywords={Computational modeling;Message passing;Education;Parallel processing;Encoding;Prompt engineering;Artificial intelligence;Programming profession;Software development management;Testing;generative AI;computing education;parallel and distributed computing},
  doi={10.1109/IPDPSW66978.2025.00107},
  ISSN={2995-066X},
  month={June},}@ARTICLE{10113742,
  author={Tian, Jinkai and Sun, Xiaoyu and Du, Yuxuan and Zhao, Shanshan and Liu, Qing and Zhang, Kaining and Yi, Wei and Huang, Wanrong and Wang, Chaoyue and Wu, Xingyao and Hsieh, Min-Hsiu and Liu, Tongliang and Yang, Wenjing and Tao, Dacheng},
  journal={IEEE Transactions on Pattern Analysis and Machine Intelligence}, 
  title={Recent Advances for Quantum Neural Networks in Generative Learning}, 
  year={2023},
  volume={45},
  number={10},
  pages={12321-12340},
  abstract={Quantum computers are next-generation devices that hold promise to perform calculations beyond the reach of classical computers. A leading method towards achieving this goal is through quantum machine learning, especially quantum generative learning. Due to the intrinsic probabilistic nature of quantum mechanics, it is reasonable to postulate that quantum generative learning models (QGLMs) may surpass their classical counterparts. As such, QGLMs are receiving growing attention from the quantum physics and computer science communities, where various QGLMs that can be efficiently implemented on near-term quantum machines with potential computational advantages are proposed. In this paper, we review the current progress of QGLMs from the perspective of machine learning. Particularly, we interpret these QGLMs, covering quantum circuit Born machines, quantum generative adversarial networks, quantum Boltzmann machines, and quantum variational autoencoders, as the quantum extension of classical generative learning models. In this context, we explore their intrinsic relations and their fundamental differences. We further summarize the potential applications of QGLMs in both conventional machine learning tasks and quantum physics. Last, we discuss the challenges and further research directions for QGLMs.},
  keywords={Quantum computing;Biological neural networks;Computational modeling;Machine learning;Physics;Integrated circuit modeling;Computers;Generative learning;quantum generative learning;quantum machine learning;quantum computing},
  doi={10.1109/TPAMI.2023.3272029},
  ISSN={1939-3539},
  month={Oct},}@INPROCEEDINGS{9182639,
  author={Jia, Zihui and Zhu, Yuesheng},
  booktitle={2020 IEEE International Conference on Artificial Intelligence and Computer Applications (ICAICA)}, 
  title={Residual Adaptive Mask Generative Adversarial Network for Image Raindrop Removal}, 
  year={2020},
  volume={},
  number={},
  pages={517-521},
  abstract={Single image raindrop removal is an extremely challenging task since the raindrop regions of various shapes and sizes are not given and the background information of the occluded regions is completely lost for most part. In this paper, a novel two-stage residual adaptive mask generative adversarial network (RAM-GAN) is developed for single image raindrop removal, in which the raindrop regions can be automatically detected and a restored image without raindrops is generated. Moreover, the residual adaptive mask block (RAMB) structures and residual dense adaptive mask modules (RDAMM) are proposed to be the main components constructing the network. The proposed RAMB structure can serve as a feature selector which adaptively enhances the effective information and suppress the invalid information. Each block is processed into two branches: soft mask branch and trunk branch. A mask is generated by the soft mask branch to softly weigh the features processed by the trunk branch. In addition, RDAMM, the residual densely connected module based on RAMB structure, is proposed to maximize the information flow among different blocks and guarantee better convergence. Our experimental results have demonstrated that our method can effectively remove raindrops while well preserving the image details, which outperforms the state-of-the-art methods quantitatively and qualitatively.},
  keywords={Adaptive systems;Image restoration;Feature extraction;Generative adversarial networks;Training;Task analysis;Image reconstruction;Image Raindrop Removal;Image Restoration;Generative Adversarial Network},
  doi={10.1109/ICAICA50127.2020.9182639},
  ISSN={},
  month={June},}@INPROCEEDINGS{11046181,
  author={Schweisthal, Brigitte and Bonciog, Dumitru-Daniel and Lascu, Mihaela and Ordodi, Valentin Laurenţiu},
  booktitle={2025 6th International Conference on Bio-engineering for Smart Technologies (BioSMART)}, 
  title={A Survey on AI-Powered Ophthalmology: A Revolution in Eye Care and Disease Management}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Artificial Intelligence (AI) is transforming ophthalmology by enhancing diagnostic accuracy, optimizing treatment strategies, and expanding access to quality eye care. AI-powered machine learning and deep learning models analyze vast ophthalmic datasets, enabling the early detection of diseases such as glaucoma, diabetic retinopathy, cataracts and age-related macular degeneration (AMD). These technologies improve image interpretation from fundus photography and optical coherence tomography, providing ophthalmologists with precise assessments of retinal health and disease progression. Beyond diagnostics, AI contributes to ophthalmic surgery by increasing precision in robotic-assisted procedures and enhancing preoperative planning. AI-driven teleophthalmology further reduces healthcare gaps by enabling remote screenings and consultations, particularly in low-access regions. Furthermore, AI-based automation improves clinical workflows, reducing administrative burdens and optimizing resource allocation. Despite its advantages, AI adoption in ophthalmology faces challenges, including data privacy concerns, algorithmic bias, and the need for regulatory approval. For widespread implementation, it is fundamental to ensure diverse, high-quality datasets and achieve seamless integration with existing healthcare systems. This study explores AI-driven advancements in ophthalmology, highlighting current applications, benefits, limitations, and future research directions. By leveraging AI, the field of ophthalmology is poised for a paradigm shift, improving patient outcomes and revolutionizing eye care globally.},
  keywords={Deep learning;Macular degeneration;Diabetic retinopathy;Sensitivity;Accuracy;Surgery;Retina;Ophthalmology;Diseases;Testing;ophthalmology;early disease detection;machine learning;deep learning;artificial intelligence;AI in healthcare},
  doi={10.1109/BioSMART66413.2025.11046181},
  ISSN={2831-4352},
  month={May},}@ARTICLE{10460408,
  author={Chowdhury, Wasif Shafaet and Yan, Yong},
  journal={Cybernetics and Intelligence}, 
  title={Applications of artificial intelligence to instrumentation systems for monitoring complex industrial processes}, 
  year={2024},
  volume={},
  number={},
  pages={1-18},
  abstract={Instrumentation and Measurement (I&M) is a field which is constantly developing due to the emergence of new technologies. In recent years, with the rapid development in computer hardware and computational power, Artificial Intelligence (AI) has demonstrated remarkable successes in data analytics, thus offering new paradigm for the design and applications of new instruments and measurement systems. The applications of AI to I&M have made measurements of some quantities in industry possible or more cost-effective. This paper presents a review of recent AI based methods applied in different aspects of instrumentation systems for monitoring complex industrial processes with a particular focus on multiphase flow metering, combustion monitoring as well as carbon dioxide flow measurement under carbon capture and storage conditions. This review also explores how AI is playing an important role in expanding knowledge across the whole spectrum of I&M. Trends and future developments of AI methods in the field of I&M are also discussed.},
  keywords={Artificial intelligence;Data models;Monitoring;Combustion;Instruments;Prediction algorithms;Neural networks;Artificial intelligence;Machine learning;Multiphase flow metering;Combustion monitoring;Carbon capture and storage},
  doi={10.26599/CAI.2024.9390006},
  ISSN={2995-9128},
  month={},}@INPROCEEDINGS{10708856,
  author={Li, Chunjiang},
  booktitle={2024 3rd International Conference on Artificial Intelligence and Autonomous Robot Systems (AIARS)}, 
  title={Design of Digital Intelligent Image Art Creation and Collection System Based on AIGC}, 
  year={2024},
  volume={},
  number={},
  pages={297-301},
  abstract={This article designs a digital intelligent image art creation collection system based on AIGC, aiming to achieve user assistance and innovation in art creation through artificial intelligence technology. The system uses the Generative Adversarial Network (GAN) algorithm and the DCGAN (Deep Convolutional GAN) model as the basic architecture of the generator and discriminator. The article trains and tunes the model based on the art image data set, and adopts appropriate hyperparameter settings and loss function design, which can generate realistic artistic images and provide a variety of artistic style selection and parameter adjustment functions, providing users with an interface for artistic style selection and parameter adjustment to meet the creative needs of different users. Through experimental evaluation, the system image acquisition time is approximately 2–8 seconds, and the system shows good performance and diversity in generating realistic artistic images.},
  keywords={Technological innovation;Art;User interfaces;Generative adversarial networks;User experience;Real-time systems;Generators;Data models;Autonomous robots;Image Acquisition;Digital Intelligent Image;AIGC;Image Retrieval},
  doi={10.1109/AIARS63200.2024.00062},
