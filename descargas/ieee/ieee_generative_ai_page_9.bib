  year={2020},
  volume={},
  number={},
  pages={454-456},
  abstract={Recently, many deep learning models for missing data imputation have been studied. One of the most popular models is Generative Adversarial Networks (GANs), which generate plausible fake data through adversarial training. In this paper, we take a look at the architecture, objective of a generator and a discriminator, training method and loss function. After that, we can see what improvements have been made to each model. Moreover, we can easily compare several GAN-based models for missing data imputation.},
  keywords={Generators;Gallium nitride;Generative adversarial networks;Data models;Machine learning;Numerical models;Training;Missing data imputation;Generative Adversarial Networks (GANs);Adversarial training;Generator;Discriminator},
  doi={10.1109/ICAIIC48513.2020.9065044},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10747963,
  author={Aryfiyanto, Hary and Alamsyah, Andry},
  booktitle={2024 7th International Conference of Computer and Informatics Engineering (IC2IE)}, 
  title={Public Service with Generative AI: Exploring Features and Applications}, 
  year={2024},
  volume={},
  number={},
  pages={1-7},
  abstract={Digitalizing public administration is crucial for leveraging advanced technologies like artificial intelligence to improve governance and public service delivery. However, the impact of generative AI on government operations is still uncertain, highlighting the need for further exploration of its potential benefits. Empirical research on the adoption of generative AI in the public sector has been slow, with limited consolidation of findings so far. Recently, generative AI has emerged with the potential to transform governmental operations by revolutionizing public sector systems and optimizing various functions. This study aims to identify the key features and applications of generative AI, assess its potential benefits and challenges, and offer strategic insights and recommendations for stakeholders to effectively integrate generative AI into the public sector. Our findings suggest that generative AI can streamline workflows, increase efficiency, and enhance decision-making. This study contributes to a clearer understanding of the role of generative AI in government, facilitating its smoother integration and more effective use in public sector settings.},
  keywords={Ethics;Technological innovation;Uncertainty;Generative AI;Navigation;Government;Decision making;Transforms;Stakeholders;Informatics;generative AI;government sector;public service;taxonomy;features;applications},
  doi={10.1109/IC2IE63342.2024.10747963},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10743943,
  author={Singh, Jasdeep and Islam, Sardar M. N. and Tewatia, Manav and Garg, Deepanshu and Fatima, Neda},
  booktitle={2024 International Conference on Advances in Computing Research on Science Engineering and Technology (ACROSET)}, 
  title={Systematic Review of GAN for Enhancing Efficiency in AI in Gaming}, 
  year={2024},
  volume={},
  number={},
  pages={1-8},
  abstract={A thorough analysis of Generative Adversarial Networks (GANs) and how they might be used to improve AI gaming efficiency is presented in this research. Game developers now have never-before-seen possibilities for innovation, immersion, and personalization thanks to GANs, which have become a potent tool for producing realistic data samples across a variety of fields. This review investigates the architecture, training methods, applications, difficulties, and potential future directions of GANs in AI gaming through a thorough analysis of the body of research and publications. The review demonstrates how GANs can transform the game industry by highlighting their many uses in procedural content generation, texture synthesis, character animation, environment design, and content customization. Nevertheless, there are certain difficulties in integrating GANs into AI games, such as computational resources, mode collapse, training instability, and assessment metrics.as well as legal matters. To tackle these obstacles, multidisciplinary research endeavors and cooperation across academic institutions, corporate entities, and regulatory agencies are necessary. Going forward, new avenues for study and developments in GAN technology hold great potential for getting over these roadblocks and opening up fresh possibilities in AI gaming. We can create a future where AI powered gaming experiences change the parameters of interactive entertainment and provide players globally with more engaging, varied, and personalized gaming experiences by utilizing the possibilities of GANs and addressing the related obstacles.},
  keywords={Training;Visualization;Technological innovation;Procedural generation;Refining;Transforms;Propulsion;Generative adversarial networks;Stakeholders;Artificial intelligence;Generative Adversarial Networks (GANs);Artificial Intelligence (AI);Gaming;Video Games;Efficiency;Enhancement;Machine Learning},
  doi={10.1109/ACROSET62108.2024.10743943},
  ISSN={},
  month={Sep.},}@INBOOK{11052340,
  author={Prabakaran, D. and Senthil, Kumaran R. and Shyamala, R.},
  booktitle={Math Optimization for Artificial Intelligence: Heuristic and Metaheuristic Methods for Robotics and Machine Learning}, 
  title={14 A Novel African Wild Dog Optimization (AWDO) Algorithm for Applications of Artificial Intelligence}, 
  year={2025},
  volume={},
  number={},
  pages={303-316},
  abstract={Artificial intelligence (AI) harbors the potential to tackle intricate challenges and enhance human capabilities across various domains. Through its significant capabilities, AI plays a crucial role in data analysis, natural language processing (NLP), healthcare, and other decision-based fields. The primary objective of AI in these domains is to render precise decisions promptly. The attainment of higher decisionmaking accuracy and system performance enhancement necessitates the utilization of optimization algorithms to aid in the decision-making process efficiently. Swarm optimization draws inspiration from the collective behavior of animals or insects that cooperate in hunting, thereby facilitating rapid decision-making. This chapter delves into diverse swarm optimization algorithms that support decision-making processes in AI applications and introduces a novel algorithm called African wild dog optimization (AWDO). The proposed AWDO algorithm is an energy-efficient swarm optimization technique that minimizes energy consumption by AI nodes or neurons, resulting in expedited search outcomes. The efficacy of the AWDO algorithm is evaluated against established optimization algorithms like particle swarm optimization, ant colony optimization (ACO), artificial bee colony (ABC) optimization algorithm, firefly algorithm, and bacterial foraging optimization (BFO) algorithms in the realm of healthcare diagnosis applications.},
  keywords={},
  doi={},
  ISSN={},
  publisher={De Gruyter},
  isbn={9783111436241},
  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/11052340},}@INPROCEEDINGS{10903787,
  author={Easttom, Chuck},
  booktitle={2025 IEEE 15th Annual Computing and Communication Workshop and Conference (CCWC)}, 
  title={Malicious Use of Artificial Intelligence}, 
  year={2025},
  volume={},
  number={},
  pages={00499-00507},
  abstract={Artificial intelligence is becoming more mainstream. Artificial intelligence has been used in medical diagnostics, detecting financial fraud, managing smart grids, and many other beneficial applications. Large language models (LLMs) are a type of artificial intelligence designed to understand and generate human language. Tools such as ChatGPT, Google's Gemini, Meta's Llama, and xAI's Grok have been widely used in a variety of contexts. However, there are malicious applications of artificial intelligence that are already being used. Artificial intelligence has been used to generate child sexual exploitation material (CSAM). Artificial intelligence being used to facilitate cyber-attacks is a growing concern. Deep fakes have been used to perpetrate elaborate financial frauds. There is also the concern of deep fakes being used to influence elections. Large language models have already been shown to create more believable phishing emails than human threat actors. Perhaps more disturbing, there have been studies showing LLMs providing guidance in planning and execution of a biological attack. AI can be used to identify and exploit vulnerable individuals online, such as for human trafficking, sexual exploitation, or labor exploitation by analyzing social media profiles and patterns of behavior. The current study analyzes the major malicious uses of artificial intelligence, including specific actual cases, and provides an overview of steps to mitigate these uses.},
  keywords={Deepfakes;Social networking (online);Large language models;Voting;Terrorism;Finance;Fraud;Smart grids;Artificial intelligence;Cyberattack;LLM;AI Malware;Deep Fakes;AI CSAM},
  doi={10.1109/CCWC62904.2025.10903787},
  ISSN={},
  month={Jan},}@INPROCEEDINGS{11059162,
  author={Bale, Ajay Sudhir and Saranya, S and B C, Santhrupth and T S, Arulananth and Satheesha, T Y and Ponnuru, Sudhakiran},
  booktitle={2024 International Conference on Advances in Computing, Communication and Materials (ICACCM)}, 
  title={Revolutionizing Chip Design using Generative AI in Semiconductors: A Case Study Approach}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={The semiconductor business is a major force behind innovation in technology, and artificial intelligence (AI) has the potential to significantly increase both output and effectiveness within the sector. AI has the potential to drastically cut costs, improve the effectiveness of manufacturing, and improve product quality by decreasing faults and optimizing use of materials. Though there is growing enthusiasm for AI applications for the semiconductor sector, thorough evaluations that methodically evaluate the body of research and pinpoint the potential and problems in this area are still absent. By giving a comprehensive overview of AI-driven methods for semiconductor production optimization and providing insightful information for future research paths, this study seeks to close this knowledge gap. This study offers information about the individual AI approaches used and their influence on various phases of semiconductor production by looking at previous research and real-world examples. This study is to educate students and shine the light on work undertaken in this field by revealing the different uses of AI, pointing out current shortcomings, and offering pertinent ideas. We examine how AI is utilized to solve architectural difficulties, optimize efficiency, and drive development in chip creation via an assortment of 20 cases including top businesses like Intel, Samsung, NVIDIA, IBM, TSMC, and more.},
  keywords={Technological innovation;Generative AI;Force;Production;Product design;Quality assessment;Manufacturing;Artificial intelligence;Optimization;Business;Chip technology;AI;computational speed;architecture;optimization},
  doi={10.1109/ICACCM61117.2024.11059162},
  ISSN={2642-7354},
  month={Nov},}@INPROCEEDINGS{10983455,
  author={Ansari, Anas and Ansari, Abdullah},
  booktitle={2025 International Conference for Artificial Intelligence, Applications, Innovation and Ethics (AI2E)}, 
  title={Exploring the Multifaceted Applications of Artificial Intelligence in Earthquake Engineering: A Comprehensive Review of Literature, Techniques, and Future Trends}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={This comprehensive review investigates the multifaceted applications of Artificial Intelligence (AI) within the field of earthquake engineering, highlighting its transformative potential and diverse methodologies. As seismic events pose significant risks to infrastructure and public safety, the integration of AI technologies has emerged as a vital approach to enhancing earthquake resilience. This study synthesizes existing literature to explore various AI applications, including Machine Learning (ML), Deep Learning (DL), and data-driven techniques, in critical areas such as seismic hazard assessment, structural health monitoring, damage detection, and post-earthquake recovery. The findings aim to provide a holistic understanding of how AI can revolutionize earthquake engineering practices, ultimately contributing to improved safety and resilience in regions vulnerable to seismic activity. Through this exploration, the paper emphasizes the need for continued research and innovation in AI applications to mitigate the impacts of earthquakes effectively.},
  keywords={Technological innovation;Ethics;Reviews;Earthquake engineering;Market research;Public security;Hazards;Artificial intelligence;Monitoring;Resilience;Artificial intelligence;earthquake engineering;early warming;damage prediction;vision-based seismic damage assessment;structural characterization},
  doi={10.1109/AI2E64943.2025.10983455},
  ISSN={},
  month={Feb},}@ARTICLE{10485414,
  author={Chaki, Jyotismita and Woźniak, Marcin},
  journal={IEEE Access}, 
  title={Deep Learning and Artificial Intelligence in Action (2019–2023): A Review on Brain Stroke Detection, Diagnosis, and Intelligent Post-Stroke Rehabilitation Management}, 
  year={2024},
  volume={12},
  number={},
  pages={52161-52181},
  abstract={Brain stroke is a complicated disease that is one of the foremost reasons of long-term debility and mortality. Because of breakthroughs in Deep Learning (DL) and Artificial Intelligence (AI) which enable the automated detection and diagnosis of brain stroke as well as intelligently assisting post-brain stroke patients for rehabilitation, is more favorable than a manual diagnosis. Many publications on automated brain stroke detection, diagnosis, and robotic management using DL and AI approaches are now being published. This review provides a study of the detection, diagnosis of brain stroke and robotic management techniques of post-brain stroke rehabilitation from six different perspectives, namely, brain stroke datasets and modalities of brain stroke data collection, pre-processing approaches, DL-based detection and diagnosis of brain stroke, Al-based intelligent post brain stroke rehabilitation assistant, and performance measures. It also examines the conclusions and the consequences of the findings. There are also three ongoing research challenges in the fields of brain stroke detection and diagnosis, as well as post-brain stroke robotic treatment. For this investigation, 130 key papers from the Scopus, PubMed and Web of Science archives were chosen after a comprehensive screening method. This study gives a comprehensive overview of brain stroke detection and post-brain stroke robotic management strategies that may be useful to the scientist’s community working in the field of automatic brain stroke detection and robotic rehabilitation management.},
