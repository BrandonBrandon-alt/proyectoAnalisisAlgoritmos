  ISSN={2767-7257},
  month={Nov},}@INPROCEEDINGS{10424561,
  author={Shankar S, Siva and Thanh Hung, Bui and Chakrabarti, Prasun},
  booktitle={2023 1st International Conference on Cognitive Computing and Engineering Education (ICCCEE)}, 
  title={A Survey of Artificial Intellegence Techniques to Detect Intrusion Attack in Communication Network}, 
  year={2023},
  volume={},
  number={},
  pages={1-10},
  abstract={Networks are a huge part of modern life, and cyber security has become a crucial area of research. An intrusion detection system (IDS) is a device that examines network traffic to confirm security, availability, and integrity. Despite years of research, conventional IDSs continue to struggle with enhancing detection performance, lowering False Alarm Rates (FAR), and identifying novel assaults. Many academics have concentrated on constructing IDSs that leverage Artificial Intelligence (AI) technologies to overcome the difficulties mentioned above. With extreme accuracy, AI algorithms can reliably identify the main distinctions among normal and anomalous data. Furthermore, AI approaches are very generalizable, allowing them to identify previously undiscovered assaults. IDS methods based on ML, DL, and meta-heuristic optimization have recently been proposed as workable alternatives for spotting intrusions throughout the network. This survey presents an IDS taxonomy that classifies and summarizes AI-based IDS research using data items as the primary dimension. This article offers a thorough analysis of recent studies on the application of AI in IDSs, including surveys and suggestions for cutting-edge techniques. Following that, recent developments and advances in AI-based network IDS are explored, as well as the proposed strategy, assessment metrics, and dataset selection. This study highlighted many research obstacles and presented future direction for research in developing AI-based Network IDS using the inadequacies of the existing approaches.},
  keywords={Surveys;Computational modeling;Taxonomy;Intrusion detection;Training data;Telecommunication traffic;Artificial intelligence;Intrusion Detection System;Wireless Networks;Artificial Intelligence;Security and Privacy;Optimization;Dataset and Performance Measures},
  doi={10.1109/ICCCEE55951.2023.10424561},
  ISSN={},
  month={April},}@INPROCEEDINGS{11116941,
  author={Han, Jingxuan and Chi, Dongxiang},
  booktitle={2024 5th International Conference on Big Data, Artificial Intelligence and Internet of Things Engineering (ICBAIE)}, 
  title={Multi-Dimensional Time-Series Data Synthesis Based on Improved TimeGAN}, 
  year={2024},
  volume={},
  number={},
  pages={151-154},
  abstract={Artificial intelligence models are increasingly used in industrial applications to assist with predictive tasks. However, the complexity of industrial environments poses challenges to data collection, transmission, and storage, often leading to data loss and insufficiency. Synthetic data generation has become a common solution to augment datasets. Existing methods can be broadly categorized into traditional statistical techniques and deep generative approaches. Statistical methods explicitly model observed data, proving advantageous for simple, small-scale synthesis tasks. In contrast, deep generative methods implicitly model data and are capable of generating more complex datasets. Nevertheless, the data synthesized by deep generative models can be uncontrollable, with issues such as mode collapse. This paper proposes a novel data synthesis method based on the deep generative model TimeGAN, aimed at synthesizing multi-dimensional wind power time-series data. TimeGAN combines the architectures of GAN and AE (Autoencoder). The proposed method introduces a parallel structure of TCN (Temporal Convolutional Networks) and Transformer as the backbone of the generator and embedder. Additionally, Wasserstein divergence is used in place of KL (Kullback-Leibler) divergence to calculate loss, effectively mitigating mode collapse during training. Comparative experiments with baseline algorithms, including TimeGAN and diffusion models, demonstrate the effectiveness of the proposed method. Furthermore, this work provides new insights into predictive tasks where data collection is difficult or incomplete.},
  keywords={Training;Statistical analysis;Wind power generation;Predictive models;Transformers;Generative adversarial networks;Feature extraction;Data models;Convolutional neural networks;Synthetic data;Data Syhthesis;Temporal Convolutional Networks;Generative Adversarial Networks;Machine Learning},
  doi={10.1109/ICBAIE63306.2024.11116941},
  ISSN={},
