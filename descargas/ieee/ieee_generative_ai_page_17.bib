  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/10948920},}@INPROCEEDINGS{10692867,
  author={Bai, Hao and Luo, Linhuan and Li, Wei and Lin, Guocheng and Liu, Tong and Sun, Qizhen},
  booktitle={2024 5th International Conference on Artificial Intelligence and Electromechanical Automation (AIEA)}, 
  title={Power system data imputation using bidirectional correlated generative adversarial networks}, 
  year={2024},
  volume={},
  number={},
  pages={219-223},
  abstract={We propose a new method for missing data imputation in smart meters in the distribution power system based on Generative Adversarial Networks. Firstly, the mask matrix and time delay matrix are used to provide information about missing position and duration. Secondly, we use a form of autoencoder to solve the random generation of simple Generative Adversarial Networks. Then, a modified bidirectional Gate Recurrent Unit is leveraged as both generator and discriminator, with consideration of highly coupled features in the distribution power system. Finally, we perform the missing data imputation in a modified 38-node test feeder and the results show that our method makes full use of features in the power system and takes a more accurate imputation than other representative methods even at large missing rate and mutation.},
  keywords={Accuracy;Delay effects;Logic gates;Position measurement;Generative adversarial networks;Imputation;Time measurement;Generators;Power systems;Spatiotemporal phenomena;smart meter;power system measurement;data imputation;generative adversial networks;bidirectional gate recurrent unit},
  doi={10.1109/AIEA62095.2024.10692867},
  ISSN={},
  month={June},}@INPROCEEDINGS{9952596,
