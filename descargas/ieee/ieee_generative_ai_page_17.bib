  year={2023},
  volume={},
  number={},
  pages={823-826},
  abstract={Generative model used widely in machine learning due to its ability to learn the feature from the input and able to regenerate the same. However, for different application different models are used. Hybrid model gives more precise results in some of the area. One such model is designed for specific application of anomaly detection. Here adversarial auto encoder model is used as base model and added adversarial effect from generative adversarial network to generate image from features. The results show that the proposed model can leverage less reconstruction loss in image regeneration and its compressed representation as latent space can classify each class differently. This research study discusses about the proposed model architecture and various methods used for hyper parameter tuning.},
  keywords={Solid modeling;Three-dimensional displays;Image coding;Computational modeling;Computer architecture;Machine learning;Generative adversarial networks;Image reconstruction;Tuning;Videos;Auto encoder;Generative model;Anomaly detection},
  doi={10.1109/ICAAIC56838.2023.10140940},
  ISSN={},
  month={May},}@INPROCEEDINGS{11022290,
  author={Peng, Qingao and Yi, Hui and Jiang, Shangjun},
  booktitle={2024 8th Asian Conference on Artificial Intelligence Technology (ACAIT)}, 
  title={Fault Diagnosis of Bearings Based on One-Dimensional Deep Convolutional Generative Adversarial Network Method}, 
  year={2024},
  volume={},
  number={},
  pages={835-839},
  abstract={In response to the difficulty of obtaining a large number of fault data samples in practical engineering applications, as well as the problem of low fault detection accuracy under small sample conditions, a bearing fault diagnosis method based on one-dimensional deep convolutional generative adversarial networks is proposed. According to the sample data of bearing vibration signals, a one-dimensional deep convolutional generative adversarial network is used to supplement the samples to enrich the fault information. Then, the processed one-dimensional time series signals are transformed into two-dimensional feature signals using Gramian Angular Field and input into the AlexNet network to achieve accurate classification of different fault types. Validation is conducted using rolling bearing data from Case Western Reserve University, and experimental results show that under small sample conditions, accurate classification can be achieved for various fault types. Compared with some existing advanced methods, it has a higher fault classification accuracy and stronger generalization performance.},
  keywords={Fault diagnosis;Vibrations;Training;Accuracy;Image coding;Convolution;Frequency-domain analysis;Time series analysis;Rolling bearings;Generative adversarial networks;sample augmentation;one-dimensional deep convolution;generative adversarial network;gramian angular field},
  doi={10.1109/ACAIT63902.2024.11022290},
  ISSN={},
  month={Nov},}@INBOOK{10948920,
  author={Banafa, Ahmed},
