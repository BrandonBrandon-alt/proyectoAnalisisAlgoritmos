  number={},
  pages={48-49},
  abstract={Artificial Intelligence (AI) has been recently attracting a lot of research efforts to develop accurate and computationally-fast methodologies for electromagnetic (EM) imaging. This work overviews some of the most recent advances in this emerging field, by also envisaging some future trends and research directions according to the authors' vision.},
  keywords={Antenna measurements;Deep learning;Imaging;Market research;Artificial intelligence;Electromagnetics;Electromagnetic (EM) Imaging;Inverse Problems (IPs);Three-Steps Learning-by-Examples (3-LBE) System-by-Design (SbD);Physics-Embedded Deep Learning (PE-DL)},
  doi={10.1109/CAMA57522.2023.10352857},
  ISSN={2643-6795},
  month={Nov},}@INPROCEEDINGS{11085644,
  author={Lakshmi, Appana Naga and Savitha, Sakala Naga and Jhansi, Nakkainti and Reddy, Kummetha Nithin Kumar and Reddy, P Manohar},
  booktitle={2025 6th International Conference on Intelligent Communication Technologies and Virtual Mobile Networks (ICICV)}, 
  title={Medical Plant Identification and Utilities Recommendation using Ensemble Techniques}, 
  year={2025},
  volume={},
  number={},
  pages={623-627},
  abstract={This paper presents a deep learning-based system capable of classifying medicinal and Ayurvedic plants with high accuracy, countering the increasing decoupling with traditional knowledge as a result of increasing urbanization. Unlike traditional identification methods, which are generally manual and expert-dependent, the solution presented here provides high-accuracy classification as well as informative utility. The plant is not just identified, but other details like its scientific name, medicinal applications, and suggested home remedies are also provided. The system is implemented using a hybrid ensemble of InceptionV3 and ConvNeXtTiny models, trained on a heterogeneous data set of 30 medicinal plants in order to provide secure and reliable classification.To recommend appropriate treatments, a collaborative filtering approach is used to compare the identified plant to Ayurvedic treatments in a structured CSV database. The system is accessed via a Streamlit-based web portal and an Android smartphone app, enabling real-time, on-the-spot plant identification and remedy recommendation. Model performance is measured in terms of accuracy, which decides its deployment readiness. With the integration of traditional Ayurvedic practices and modern artificial intelligence, this system provides a pragmatic, user-friendly tool for promoting the use of natural medicine and public awareness of plant-based health remedies.},
  keywords={Medicinal plants;Deep learning;Accuracy;Databases;Feature extraction;Real-time systems;Data models;Reliability;Ensemble learning;Biomedical imaging;Medicinal Plants;Ayurvedic Remedies;Ensemble Methods;Deep Learning;Personalized Recommendations},
  doi={10.1109/ICICV64824.2025.11085644},
  ISSN={},
  month={June},}@INPROCEEDINGS{10687906,
  author={Gadiyar, H. Manoj T. and Kumar, S Vinod and Raj, Vijilius Helena and Yadav, Dinesh Kumar and Hasoni, Mortada Kareem Sabi and Lakhanpal, Sorabh},
  booktitle={2024 OPJU International Technology Conference (OTCON) on Smart Computing for Innovation and Advancement in Industry 4.0}, 
  title={Next-Generation Artificial Intelligence Solutions for Comprehensive Analysis and Improvement of Heat Dissipation in High-Intensity Boiling Environments}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={This research delves into the realm of AI-driven heat dissipation, offering a suite of algorithms to address the challenges posed by high-intensity boiling environments. The proposed predictive modeling algorithm, rooted in linear regression, provides a systematic and robust approach through 20 meticulously designed steps. This algorithm, along with neural networks, reinforcement learning, generative adversarial networks (GANs), and genetic algorithms, forms a versatile toolbox for thermal management. Linear regression, through its step-by-step predictive modeling process, ensures accurate heat dissipation predictions. Neural networks, employing backpropagation, showcase adaptability and complexity, excelling in real-time adaptation. Reinforcement learning, specifically Q-Learning, introduces dynamic decision-making strategies, vital for navigating complex thermal scenarios. GANs contribute by generating realistic synthetic data, an invaluable asset for simulating high intensity boiling environments. Genetic algorithms, resembling natural selection, prove versatile and scalable in evolving optimal solutions. The research evaluates these algorithms comprehensively, demonstrating the proposed method’s superiority in prediction accuracy, energy efficiency, adaptability, and overall performance. The proposed predictive modeling algorithm stands out as a benchmark, showcasing its potential for practical implementation and optimization in diverse scenarios. This comprehensive exploration sets the stage for innovative solutions to the intricate challenges of thermal management, as continuous adaptation and fine-tuning remain pivotal in this dynamic landscape.},
  keywords={Heating systems;Adaptation models;Accuracy;Heuristic algorithms;Neural networks;Linear regression;Predictive models;Prediction algorithms;Optimization;Genetic algorithms;Adaptive Prediction;AI-driven Heat Dissipation;Collaborative Optimization;Computational Complexity;Environmental Impact;Generative Adversarial Networks (GANs);Gradient Descent;Thermal Management;Weight Optimization},
  doi={10.1109/OTCON60325.2024.10687906},
  ISSN={},
  month={June},}@INPROCEEDINGS{9977500,
  author={Li, Jianxue and Xiao, Yang and Wu, Jiawei and Chen, Yaozhi and Liu, Jun},
  booktitle={2022 IEEE 33rd Annual International Symposium on Personal, Indoor and Mobile Radio Communications (PIMRC)}, 
  title={Attentive Dual-Head Spatial-Temporal Generative Adversarial Networks for Crowd Flow Generation}, 
  year={2022},
  volume={},
  number={},
  pages={800-806},
  abstract={Crowd flow generation, which aims to simulate the flows of crowd in the future is of great importance to many real-life applications including epidemic spreading and traffic management. The challenges of accurate crowd flow generation come from both the complex spatial-temporal correlations of crowd flow data and the small amount of training data, which is largely not well studied and addressed in existing works. In this paper, we propose a novel attentive dual-head spatial-temporal generative adversarial network entitled ADST-GAN to simulate multi-step crowd flow. To augment the learning power of the generator, we adopt attentive temporal queue and self attention mechanism to automatically capture the complex global spatial-temporal dependencies of crowd flow data. As for the discriminator, we design a dual-head architecture with two-objective training to avoid the negative effects of quick overfitting. To evaluate the effectiveness of the proposed method, we conduct extensive experiments over the bike and taxi trip datasets in New York. The results demonstrate the proposed method outperforms seven state-of-the-art baselines significantly in terms of the quality of simulated crowd flow data.},
  keywords={Training;Epidemics;Correlation;Training data;Predictive models;Generative adversarial networks;Feature extraction;Crowd flow generation;generative adversarial networks;attention mechanism},
  doi={10.1109/PIMRC54779.2022.9977500},
  ISSN={2166-9589},
  month={Sep.},}@INPROCEEDINGS{10739305,
  author={Chandila, Rishabh and Kumar, Deepak},
  booktitle={2024 International Conference on Electrical Electronics and Computing Technologies (ICEECT)}, 
  title={Conversion of Text-to-Image Generation}, 
  year={2024},
  volume={1},
  number={},
  pages={1-5},
  abstract={With the exponential growth of data and advancements in Artificial Intelligence (AI), the task of converting textual descriptions into visual images has gained significant traction. This research addresses the challenge by leveraging AI techniques to develop a robust Text-to-Image Generation system that translating text prompt into accurate, high-quality images. This study aims to explore and analyze cutting-edge AI techniques for text-to-image generation, with a focus on enhancing the accuracy and visual quality of generated images. Our strategy involves employing state-of-the-art neural network architectures and evaluation metrics to ensure high-quality image synthesis. The significance of this project lies in its potential to revolutionizing industries from digital art to product design. Through this endeavor, we aim to contribute to the frontier of AI-driven imaginary image generation.},
  keywords={Visualization;Technological innovation;Solid modeling;Accuracy;Three-dimensional displays;Image synthesis;Text to image;Transformers;Weaving;Videos;Text-to-image Generation;Artificial Intelligence (AI);Natural Language Processing (NLP);Image Synthesis;Generative Models},
  doi={10.1109/ICEECT61758.2024.10739305},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{10920248,
  author={Wu, Ye and Wei, Qinggong and Song, Ruiqi and Cui, Chenglin and Li, Xinqing and Zhu, Fenghua and Ai, Yunfeng},
  booktitle={2024 IEEE 27th International Conference on Intelligent Transportation Systems (ITSC)}, 
  title={A Generative Active Learning Framework for Semantic Segmentation in Autonomous Driving}, 
  year={2024},
  volume={},
  number={},
  pages={2979-2984},
  abstract={Active learning is a machine learning or artificial intelligence method that aims to achieve optimal model performance by actively selecting and labeling the most valuable samples, using the fewest high-quality labeled samples. Relevant works have already been conducted using information uncertainty to select valuable samples. However, its effectiveness for semantic segmentation tasks is relatively poor. In this paper, we propose an active learning framework based on the Generative Adversarial Networks (GANs) architecture for semantic segmentation task, which achieves efficient data selection while simultaneously enhancing semantic segmentation performance. The proposed framework combines the generator (i.e., semantic segmentation network) and the discriminator (i.e., data selection network) for jointly optimizing of both semantic segmentation and data selection tasks. Extensive experiments were conducted on the dataset, which was collected from real-world autonomous driving scenes. Experimental results demonstrate that our method achieved competitive results compared to other SOTA methods.},
  keywords={Uncertainty;Semantic segmentation;Active learning;Generative adversarial networks;Diffusion models;Generators;Labeling;Iterative methods;Autonomous vehicles;Intelligent transportation systems;Active learning;autonomous driving;semantic segmentation;generative adversarial networks},
  doi={10.1109/ITSC58415.2024.10920248},
