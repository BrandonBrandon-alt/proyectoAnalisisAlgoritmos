  author={Elena Iliuță, Miruna and Pop, Eugen and Moisescu, Mihnea-Alexandru and Ştefan Sacală, Ioan and Mitulescu, Traian-Costin},
  booktitle={2025 IEEE International Conference on Engineering, Technology, and Innovation (ICE/ITMC)}, 
  title={Generation, Analysis and Validation of Retinal Images Associated with Diabetic Retinopathy Using Generative Artificial Intelligence}, 
  year={2025},
