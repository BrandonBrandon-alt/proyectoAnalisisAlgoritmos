  month={Dec},}@BOOK{10769100,
  author={Kolosnjaji, Bojan and Xiao, Huang and Xu, Peng and Zarras, Apostolis},
  booktitle={Artificial Intelligence for Cybersecurity: Develop AI approaches to solve cybersecurity problems in your organization},
