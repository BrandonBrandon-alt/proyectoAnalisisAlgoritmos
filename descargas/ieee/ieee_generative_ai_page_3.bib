  ISSN={},
  month={May},}@INBOOK{10951337,
  author={Smith, Christie and Monahan, Kelly},
  booktitle={Essential: How Distributed Teams, Generative AI, and Global Shifts Are Creating a New Human-Powered Leadership}, 
