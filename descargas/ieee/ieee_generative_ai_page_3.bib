  booktitle={2025 IEEE 23rd Jubilee International Symposium on Intelligent Systems and Informatics (SISY)}, 
  title={The Practical Usage of Artificial Intelligence in Pedagogical Practice: A Study of Teachers' Usage Habits in Hungary}, 
  year={2025},
  volume={},
  number={},
  pages={000271-000278},
  abstract={The advancement of digital technologies has introduced significant challenges to educational systems, affecting both primary and higher education institutions. While the COVID-19 pandemic accelerated the adoption of digital tools in education, the transformation largely remained superficial, focusing primarily on converting materials to digital formats and conducting virtual classes rather than implementing comprehensive digital pedagogical reform. The emergence of generative artificial intelligence technologies has introduced both unprecedented opportunities and complex challenges to educational environments, potentially exacerbating existing systemic issues. These AI-powered tools offer substantial benefits, including customized learning experiences, real-time analysis of student responses, and streamlined administrative processes. However, students have rapidly adopted these technologies independently, often utilizing them for various academic purposes without proper guidance. Despite the transformative potential of generative AI and its widespread availability, educational institutions have been slow to develop comprehensive responses to this technological shift. A significant gap exists in quality resources that would enable educators to effectively harness these tools while maintaining a balanced perspective on their capabilities and limitations. Furthermore, there are insufficient professional development opportunities focused on helping teachers guide students and parents in understanding and appropriately using these emerging technologies. This study, conducted in late 2024, investigates the current state of AI tool adoption among educators, examining their classroom applications, implementation challenges, and overall experiences. The research aims to provide insights into how teaching professionals are currently engaging with generative AI technologies and to identify the barriers they face in effectively integrating these tools into their pedagogical practice.},
  keywords={Learning systems;Technological innovation;Translation;Generative AI;Pandemics;Education;Real-time systems;Artificial intelligence;Intelligent systems;Informatics;Generative AI;Skills;Teachers;Education;Tools},
  doi={10.1109/SISY67000.2025.11205410},
  ISSN={1949-0488},
  month={Sep.},}@INPROCEEDINGS{9679227,
  author={Song, Zhenlin and Sun, Linyun},
