  booktitle={2024 IEEE Region 10 Symposium (TENSYMP)}, 
  title={Advancement in 3-D Medical Images Using Artificial Intelligence Techniques}, 
  year={2024},
  volume={},
  number={},
  pages={1-8},
  abstract={This paper presents a comprehensive review of recent advancements in 3D medical imaging utilizing artificial intelligence (AI) techniques. The review encompasses various studies focused on different applications and types of 3D medical imaging modalities. The literature review discusses works such as deep learning models for brain tumor segmentation in MRI data, active learning strategies for COVID-19 infection segmentation in CT scans, multi-class point cloud completion networks for cardiac anatomy reconstruction from cine MRI images and deep CNN for total brain volume estimation from 3D ultrasound images, among others. Additionally, the paper includes a detailed analysis of techniques and outcomes, summarized in a table, providing insights into the efficacy of different AI approaches in medical imaging tasks. Moreover, the applications and types of 3D medical imaging modalities discussed in the reviewed studies span across various domains including oncology and surgery.},
  keywords={Image segmentation;Three-dimensional displays;Ultrasonic imaging;Reviews;Magnetic resonance imaging;Volume measurement;Oncology;Artificial intelligence;Medical diagnostic imaging;Image reconstruction;3D medical imaging;Artificial intelligence;Convolutional neural network;Segmentation;Deep learning},
  doi={10.1109/TENSYMP61132.2024.10752259},
  ISSN={2642-6102},
  month={Sep.},}@INPROCEEDINGS{10151603,
  author={Jadhav, Ranjana and Gokhale, Vedant and Deshpande, Mihir and Gore, Aayush and Gharpure, Adwait and Yadav, Harsh},
