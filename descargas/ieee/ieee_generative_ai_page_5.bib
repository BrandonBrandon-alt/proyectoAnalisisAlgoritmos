  booktitle={2023 31st Telecommunications Forum (TELFOR)}, 
  title={Exploring the Potential of Generative Adversarial Networks for Synthetic Medical Data Generation}, 
  year={2023},
  volume={},
  number={},
  pages={1-4},
  abstract={Proper artificial intelligence (AI) and machine learning (ML) model implementation and training are highly reliant on quality input data. However, the high costs of clinical studies and increasing privacy concerns limit the availability of medical data to researchers. This work explores the potential of applying generative adversarial networks (GAN) for generating synthetic medical data based on publicly available real-world datasets. Synthetic data is generated based on real-world observations and used to train several contemporary ML and AI models. These models are subjected to comparison with models trained on real data. Both trained models are evaluated under identical conditions on real-world testing data. These well-known, high-quality datasets are explored in this study covering diabetes and heart disease. While models trained on synthetic data show a slight decrease in objective performance, they still demonstrate viably accurate outcomes. This suggests that the introduced approach presents a potential stepping stone towards improvising data availability for researchers while respecting patients’ best interests and patient privacy.},
  keywords={Training;Industries;Data privacy;Publishing;Generative adversarial networks;Hyperparameter optimization;Data models;Generative adversarial networks;Data synthesis;Diagnosis;Medical Data;Classification},
  doi={10.1109/TELFOR59449.2023.10372727},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10956228,
  author={Harsanto, Kenzie and Zaidan, Muhammad Taqi and Lucky, Henry and Suhartono, Derwin},
  booktitle={2024 International Conference on Informatics, Multimedia, Cyber and Information System (ICIMCIS)}, 
  title={Detecting AI-Generated Artworks With Deep Learning}, 
  year={2024},
  volume={},
  number={},
  pages={472-476},
  abstract={The rapid development of artificial intelligence has made an increase in the amount of generated content around the globe. Art is the most threatened field due to AI, potentially affecting the market value of an artist's creation. These generated images also affect artists who rely on their work for income as the accessibility of these models diminishes the demand for hiring real artists. As these generative models continue to evolve, it is becoming more difficult to differentiate between authentic art and generated ones. This paper addresses the need for a tool that can accurately distinguish between human-made and AI-generated artworks using a convolutional neural network (CNN) known as EfficientNetV2, ResNet50, and VGGNet. Our methodology involves using a dataset comprising images created by both human artists and AI algorithms. This aim of developing this tool is to preserve the integrity of human-made art while also protecting artists' rights and sustaining the value of art in the market. The researched produce a satisfying result with EfficientNetV2 achieving 98.7% accuracy, ResNet50 achieving 98.5% accuracy and VGG16 achieves 97.4% accuracy. This solves our problem of identifying AI-generated artworks as artists can rest assure their integrity and originality of their work can be recognize by using this model.},
  keywords={Deep learning;Art;Accuracy;Multimedia systems;Convolutional neural networks;Artificial intelligence;Informatics;Residual neural networks;Artificial Intelligence;Art;CNN;Dataset;EjjicientNetV2;Generated art;Generative models;Human artists;Market value;ResNet50},
  doi={10.1109/ICIMCIS63449.2024.10956228},
  ISSN={2837-5203},
  month={Nov},}@INPROCEEDINGS{10867340,
  author={Khan, Ijaz and Chohan, Ikhlaq and Malik, Sohail Iqbal},
  booktitle={2024 2nd International Conference on Computing and Data Analytics (ICCDA)}, 
  title={Leveraging ChatGPT-4 for Enhanced Education: Personalized Problem-Solving and Consistent Learning}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={Generative AI has revolutionized education by providing personalized learning experiences, enabling interactive content creation, and offering innovative approaches to problem-solving, significantly enhancing student engagement and understanding of complex subjects. This research demonstrates that ChatGPT-4, a cutting-edge generative AI, can significantly enhance the educational process, particularly in complex subjects like Machine Learning. By customizing ChatGPT-4 into a robot to replicate specific teaching methodologies, it supports students in mastering problem-solving techniques that align with their instructors' preferences. This adaptation not only aids in problem-solving but also in generating quizzes and handling diverse learning challenges in a manner consistent with classroom teachings. The key advantage of this robot lies in its ability to offer tailored learning experiences, bridging the gap between theoretical knowledge and practical application, thereby ensuring students are better prepared for examinations. The positive impact of such AI integration into education points toward a future where learning is more interactive, personalized, and effective, highlighting the transformative potential of AI in enhancing both student engagement and comprehension.},
  keywords={Data analysis;Generative AI;Computational modeling;Education;Machine learning;Learning (artificial intelligence);Data models;Multilingual;Problem-solving;Robots;Generative Artificial Intelligence;ChatGPT;AI-Driven Learning;Higher Education;Personalized learning},
