  author={Zhang, Yuzhen and Liu, Guanqun and Zhao, Yanyan and Zha, Daren and Xin, Wang and Zhao, Lin and Wang, Lei},
  booktitle={2021 IEEE 33rd International Conference on Tools with Artificial Intelligence (ICTAI)}, 
  title={FRAGAN-VSR: Frame-Recurrent Attention Generative Adversarial Network for Video Super-Resolution}, 
  year={2021},
  volume={},
  number={},
  pages={753-757},
  abstract={Video super resolution (SR) is an important task, which recovers high-resolution (HR) frames from consecutive low-resolution (LR) couterparts. The most advanced works achieved good performance to this day. However, most of them has largely focussed on making a breakthrough in accuracy and speed, which has neglect that how to recover the finer texture details. Therefore, in this paper, we first present an Video SR model combined generative adversarial network and recurrent neural network (GAN-RNN) structure. It is forced by the self-attention mechanism to pay great attention to the high-frequency information of the LR frames. The perceptual loss is introduced to retain the high-frequency detail which is different from other video SR network. A great deal of evaluations and comparisons with previous methods have confirmed the merits of the proposed framework which can significantly outperform the current state of the art.},
  keywords={Training;Recurrent neural networks;Conferences;Superresolution;Fitting;Generative adversarial networks;Task analysis;Video Super-Resolution;Generative Adversarial Network;Recurrent Neural Network;Self-attention Mechanism},
  doi={10.1109/ICTAI52525.2021.00119},
  ISSN={2375-0197},
  month={Nov},}@INPROCEEDINGS{11108586,
  author={Cui, Yuqi and Lei, Xinkun and Shang, Hua and Meng, Fanzhen},
  booktitle={2025 IEEE International Conference on Pattern Recognition, Machine Vision and Artificial Intelligence (PRMVAI)}, 
  title={Region-Guided Generative Adversarial Networks for Small Sample MRI Generation from CT in Temporal Lobe Epilepsy}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={As one of the most common neurological disorders, epilepsy poses major challenges in clinical practice. Temporal lobe epilepsy (TLE), which is highly prevalent among drug-resistant cases, often fails to respond to anti-epileptic drugs (AEDs), making surgical intervention a necessary treatment option. Consequently, accurate identification of the epileptogenic zone is essential to optimize surgical outcomes. MRI is the most widely used non-invasive imaging technique for epileptogenic focus localization, offering superior soft-tissue contrast and diagnostic accuracy. Nevertheless, the utility of MRI can be restricted due to time limitations, high costs, or certain patient-specific contraindications. Recently, the deep learning breakthroughs have made the synthesis of MRI images from CT scans possible. However, the patients undergoing both CT and MRI scans are limited, making it difficult to obtain large-scale paired datasets. In this study, we have proposed an RSPGAN method which is based on generative adversarial networks (GANs). This method has integrated image registration and temporal lobe segmentation. In the result, the images generated by the RSPGAN method achieve an SSIM of $0.97 \pm 0.02$ and a PSNR of $30.36 \pm 4.74$, with significantly reduced error metrics: MAE of $4.93 \pm 2.82$ and MSE of $79.10 \pm 43.49$, outperforming the other models. The experimental results demonstrate that our RSPGAN method achieves superior performance on limited datasets.},
  keywords={Temporal lobe;Image segmentation;Accuracy;Magnetic resonance imaging;Computed tomography;Epilepsy;Surgery;Object segmentation;Generative adversarial networks;Brain modeling;component;Synthetic MRI;CycleGAN;Generative adversarial networks;Epilepsy introduction},
  doi={10.1109/PRMVAI65741.2025.11108586},
  ISSN={},
  month={June},}@INPROCEEDINGS{11104905,
  author={Khalaily, Awham},
  booktitle={2025 International Conference on Smart Learning Courses (SCME)}, 
  title={AI-Powered Intelligent Tutoring Systems in Higher Education: A Review of Current Approaches, Challenges, and Future Directions}, 
  year={2025},
  volume={},
  number={},
  pages={30-36},
  abstract={This paper presents a systematic review of ten recent studies examining the design and application of Intelligent Tutoring Systems (ITS) in higher education. The study explores the technological foundation of modern ITS, emphasizing personalization strategies, feedback mechanisms, and implementation challenges. Findings revealed a clear shift toward systems powered by Large Language Models (LLMs) and adaptive learning architectures to deliver a more personalized, scalable, and human-like learning experience and individual support. Despite the progress, ethical concerns, transparency, and scalability remain critical challenges.What sets this study apart is its timely synthesis of interdisciplinary work that bridges AI technologies with pedagogical application in higher education. Theoretically, it contributes by identifying emerging design patterns and categorizing evolving AI techniques within ITS. Practically, it offers insights for educators, institutions, and developers seeking to adopt or design ITS that are both context-aware and pedagogically sound. This review highlights key directions for future development and responsible implementation of AI in educational settings.},
  keywords={Ethics;Adaptive learning;Technological innovation;Systematics;Generative AI;Scalability;Large language models;Educational technology;Real-time systems;Artificial intelligence;Artificial Intelligence;Intelligent Tutoring Systems;personalized learning;higher education;adaptive learning;generative AI;educational technology},
