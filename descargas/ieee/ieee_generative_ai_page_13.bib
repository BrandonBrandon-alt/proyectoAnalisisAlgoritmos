  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={This paper proposes a new approach to improve weak supervised classification of deep CNN architectures based on data augmentation by integrating Grey Wolf Optimizer (GWO) with Wasserstein Generative Adversarial Network (WGAN). The model is applied for hyperspectral image classification (HSI). We have chosen a weak supervised CNN classifier, where only $5 \%$ of hyperspectral training data is authentic, the remaining $95 \%$ ones being virtually generated using the model GWO-WGAN. The performance of the proposed model is evaluated using Indian Pines, Pavia University, and Salinas hyperspectral image datasets.},
  keywords={Training;Deep learning;Computational modeling;Training data;Computer architecture;Data augmentation;Generative adversarial networks;weak supervised classification;CNN;Grey Wolf Optimizer (GWO);Wasserstein Generative Adversarial Network (WGAN);hyperspectral image classification},
  doi={10.1109/ECAI61503.2024.10607586},
  ISSN={},
  month={June},}@INPROCEEDINGS{10392465,
