  author={Changala, Ravindra and Kayalvili, S. and Farooq, Mansoor and Rao, L Malleswara and Rao, Vuda Sreenivasa and Muthuperumal, S},
  booktitle={2024 International Conference on Data Science and Network Security (ICDSNS)}, 
  title={Using Generative Adversarial Networks for Anomaly Detection in Network Traffic: Advancements in AI Cybersecurity}, 
  year={2024},
  volume={},
