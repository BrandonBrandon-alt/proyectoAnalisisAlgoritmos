  isbn={9781394234189},
  url={https://ieeexplore-ieee-org.crai.referencistas.com/document/10952938},}@ARTICLE{10478550,
  author={Shen, Xiaobing and Zuo, Yu and Kong, Jiaze and Martinez, Wilmar},
  journal={IEEE Transactions on Power Electronics}, 
  title={Artificial Intelligence Applications in High-Frequency Magnetic Components Design for Power Electronics Systems: An Overview}, 
  year={2024},
  volume={39},
  number={7},
  pages={8478-8496},
  abstract={This article provides an overview of how artificial intelligence (AI) is applied in designing high-frequency magnetic components, primarily high-frequency inductors and transformers, for power electronics systems. Four categories of AI, including expert systems, fuzzy logic, metaheuristic methods, and machine learning techniques, are addressed. First, AI models for estimating losses in high-frequency magnetic components are discussed. Subsequently, AI-based design methods in high-frequency inductors and transformers are observed. Then, AI tools applied to the automatic design of high-frequency magnetic components are introduced and compared. Drawing insights from an analysis of over 200 publications, this article highlights significant advancements: the development of AI-driven models for precise loss estimation in high-frequency magnetic components, the application of AI in optimizing design configurations for the components, and the automation of design processes. These achievements demonstrate AI's capability to enhance the efficiency, performance, and innovation in high-frequency magnetic component design, offering a roadmap for future research in power electronics systems.},
  keywords={Artificial intelligence;Magnetosphere;Soft magnetic materials;Magnetic resonance;Saturation magnetization;Hafnium;Amorphous magnetic materials;Artificial intelligence (AI);high-frequency (HF) inductor design;HF magnetic components;HF transformer design;loss models},
  doi={10.1109/TPEL.2024.3381431},
  ISSN={1941-0107},
  month={July},}@ARTICLE{10561550,
  author={Qiu, Wangde and Zhao, Xing and Tyrrell, Andy and Perinpanayagam, Suresh and Niu, Shuangxia and Wen, Guojun},
  journal={IEEE Transactions on Power Electronics}, 
  title={Application of Artificial Intelligence-Based Technique in Electric Motors: A Review}, 
  year={2024},
  volume={39},
  number={10},
  pages={13543-13568},
  abstract={Electric motors find widespread application across various industrial fields. The pursuit of enhanced comprehensive electric motors performance has consistently drawn significant attention, prompting extensive research in this domain. With the rapid and profound integration of artificial intelligence (AI) technology into diverse sectors, an increasing array of AI-based methodologies is being employed in electric motors research. This review aims to comprehensively delineate the applications of AI within the realm of electric motors, specifically focusing into three pivotal stages: electric motors design, drive control, and health maintenance, covering a full-cycle research and development of electric motors. The review commences by outlining the elucidation of the fundamental principles and components of AI. Subsequently, the encompassed input signals, characteristic methodologies, and AI techniques are reviewed and analyzed as demonstrated by pertinent research cases. Furthermore, a synthesis of the distinctive attributes characterizing various techniques is presented. Conclusively, this article offers an unprecedented review of AI-based technology throughout the entirety of the electric motors research cycle. Considering the existing body of knowledge, this article stands as the pioneering endeavor to encapsulate the expansive domain of AI's influence in electric motors research.},
  keywords={Artificial intelligence;Electric motors;Optimization;Fuzzy logic;Reviews;Maintenance;Fault diagnosis;Artificial intelligence (AI);design;drive control;electric motors;maintenance},
  doi={10.1109/TPEL.2024.3410958},
  ISSN={1941-0107},
  month={Oct},}@INPROCEEDINGS{10685696,
  author={Li, Jiayuan and Zhu, Sha and Yang, Harrison Hao and Xu, Jian},
  booktitle={2024 International Symposium on Educational Technology (ISET)}, 
  title={What does Artificial Intelligence Generated Content bring to Teaching and Learning? A literature review on AIGC in Education}, 
  year={2024},
  volume={},
  number={},
  pages={18-23},
  abstract={In order to inspire future researches and practices regarding the use of Artificial Intelligence Generated Content (AIGC) in education, this study conducted a literature review on AIGC in education. The study found that using AIGC in teaching and learning brings unprecedented opportunities for digital transformation in education, including providing technical support for the enhancement of personalized learning, empowering the cultivation of higher-order cognitive skills, and transforming educational assessment to reshape teaching and learning. Nevertheless, the application of AIGC in education may involve risks such as cognitive biases, ethical concerns, and widening the digital divide. To address the risks, coping strategies have been proposed by researchers, including strengthening the capability of providing reliable AIGC services, developing ethical guidelines for AIGC in education, and promoting teachers’ and students’ digital literacy. However, the existing studies mainly focused on theoretical analyses, and practical researches of AIGC in education is still limited. Based upon the literature review, the authors put forward future research directions for reference.},
  keywords={Ethics;Bibliographies;Digital transformation;Education;Learning (artificial intelligence);Educational technology;Reliability theory;artificial intelligence generated content (AIGC);education;literature review;teaching and learning},
  doi={10.1109/ISET61814.2024.00013},
  ISSN={2766-2144},
  month={July},}@ARTICLE{10818563,
  author={Fang, Jiashu and Liu, Chongru},
  journal={iEnergy}, 
  title={Artificial intelligence techniques for stability analysis in modern power systems}, 
  year={2024},
  volume={3},
  number={4},
  pages={194-215},
  abstract={Effective stability analysis is essential for the secure operation of modern power systems. As smart grids evolve with increased interconnection, renewable energy integration, and electrification, the large-scale deployment of ultra-high voltage AC/DC networks introduces various operational modes and potential fault points, posing significant challenges to maintaining stability. Traditional analysis and control methods fall short under these conditions. In contrast, emerging artificial intelligence (AI) techniques, combined with real-time data collection, provide powerful tools for enhancing stability analysis in smart grids. This paper comprehensively explores AI techniques in stability analysis, discussing the necessity and rationale for integrating AI into stability analysis through the lenses of knowledge fusion, discovery, and adaptation. It provides a thorough review of current studies on AI applications in stability analysis, addresses key challenges, and outlines future prospects for AI integration, highlighting its potential to improve analytical capabilities in complex power systems.},
  keywords={Adaptation models;Renewable energy sources;Analytical models;Uncertainty;Reviews;Power system stability;Stability analysis;Data models;Smart grids;Artificial intelligence;Smart grids;artificial intelligence;deep learning;stability analysis},
  doi={10.23919/IEN.2024.0027},
  ISSN={2771-9197},
  month={December},}@ARTICLE{10679079,
  author={Koh, Eunkyung and Kim, Hyeon-Deuk and Baek, Seungin and Lee, Changhee},
  journal={IEEE Open Journal on Immersive Displays}, 
  title={A Review on the Applications of Artificial Intelligence (AI) in the Display Industry}, 
  year={2024},
  volume={1},
  number={},
  pages={165-172},
  abstract={Artificial intelligence (AI) technology is being applied to various industries such as medical care, security, education, and manufacturing, and its application is rapidly expanding. AI has been implemented in various fields of the display industry, including display design, defect analysis, and automated process control and inspection. Research on on-device applications for displays is also active. These applications aim to advance research, develop technologies, and automate and standardize manufacturing processes. This paper comprehensively reviews AI applications in the display industry.},
  keywords={Artificial intelligence;Predictive models;Organic light emitting diodes;Layout;Optical imaging;Industries;Circuits;Artificial intelligence;displays;design automation;smart manufacturing;automatic optical inspection},
  doi={10.1109/OJID.2024.3458904},
  ISSN={2836-211X },
  month={},}@INPROCEEDINGS{11039329,
  author={Ratul, Qudrat E Alahy and Zaman, Eshita and Das, Saikat and Memari, Majid},
  booktitle={2025 Intermountain Engineering, Technology and Computing (IETC)}, 
  title={The Role of AI in Transforming Education: A Systematic Review of Trends}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Artificial Intelligence (AI) has emerged as a transformative force in higher education, particularly in STEM disciplines, revolutionizing teaching, learning, and administrative processes. This survey provides a comprehensive overview of AI applications in university education, focusing on large language models (LLMs) and generative AI. Key areas include intelligent tutoring systems, automated grading, adaptive learning platforms, and AI-driven administrative support. While AI offers significant benefits, such as personalized learning and operational efficiency, it also introduces challenges, including ethical concerns, algorithmic bias, and data privacy risks. This paper critically examines these challenges and identifies future directions, such as the integration of generative AI in STEM education, advancements in natural language processing, and the development of explainable and trustworthy AI systems. By synthesizing current research, this survey aims to equip educators, policymakers, and researchers with insights into the evolving role of AI in education and its implications for creating equitable and effective learning environments.},
  keywords={Surveys;Ethics;Adaptive learning;Generative AI;Large language models;Market research;Natural language processing;Artificial intelligence;Systematic literature review;STEM;AI in education;generative AI;STEM education;personalized learning;automated grading;large language models (LLMs);adaptive learning;ethical AI;higher education},
  doi={10.1109/IETC64455.2025.11039329},
  ISSN={},
  month={May},}@INPROCEEDINGS{11075470,
  author={Baigabylov, Nurlan and Akpanov, Altynbek and Baigusheva, Kanagat and Kudabekov, Medet},
  booktitle={2025 16th International Conference on E-Education, E-Business, E-Management and E-Learning (IC4e)}, 
  title={The Impact of Digitalization on the Quality of Higher Education}, 
  year={2025},
  volume={},
  number={},
  pages={689-694},
  abstract={Nowadays, many areas of human activity, particularly higher education, are undergoing significant digitalization due to the rapid development of digital technologies and artificial intelligence. These advancements contribute to the make learning materials more accessible and support personalized education. However, alongside these advantages, digitalization also presents several challenges, such as the need to adapt educational programs and ensure academic integrityThis study examines the perception of digitalization among university students in Kazakhstan, its impact on the educational process, and the use of generative artificial intelligence in learning. Based on data analysis, key barriers to digitalization have been identified, including insufficient digital infrastructure and difficulties in integrating AI into the educational process. Recommendations are proposed to enhance the effectiveness of digital tools in the learning environment.},
