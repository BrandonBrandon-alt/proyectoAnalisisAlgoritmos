  title={Adaptive Learning in Engineering Courses: How Artificial Intelligence (AI) Can Improve Academic Outcomes}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={This qualitative research article explores the impact of Artificial Intelligence (AI) on enhancing academic performance in engineering courses, focusing on adaptive learning systems. The study highlights the evolution of digital technologies in education, emphasizing the applicability of AI in personalizing and adapting learning. Analyzing the integration of AI in engineering education, the study unveils benefits such as teaching customization and early detection of student difficulties. Concurrently, challenges are discussed, including ethical issues like data privacy and algorithmic biases. The research, adopting a qualitative approach, is grounded in an integrative literature review, considering recent studies on the application and impact of AI in higher education. The findings suggest that AI holds significant potential for transforming engineering education, provided its implementation is accompanied by ethical considerations and proper educator preparation.},
  keywords={Training;Ethics;Adaptive learning;Privacy;Data privacy;Generative AI;Bibliographies;Adaptive learning;Artificial intelligence;Engineering education;Educational technologies},
  doi={10.1109/EDUNINE60625.2024.10500580},
  ISSN={},
  month={March},}@INPROCEEDINGS{11116985,
  author={Bandla, Chakradhar},
  booktitle={2025 8th International Symposium on Big Data and Applied Statistics (ISBDAS)}, 
  title={Leveraging Generative AI for Enhanced Scalability and Efficiency in Distributed Cloud Databases}, 
  year={2025},
  volume={},
  number={},
  pages={280-286},
  abstract={Data-driven application deployment imposes needs for distributed cloud databases that need to handle extensive data quantities efficiently as they provide guarantee and scaling properties and operational performance. Distributed databases now benefit from advanced capabilities enabled by AI systems that promote better database operation and scalability capacity. This research explores the integration of generative artificial intelligence systems with distributed cloud databases to solve database query optimization challenges while managing operational workloads and making fault predictions and ensuring data consistency initiatives. Through our research on generative AI's predictive functions and generative capabilities we formulate innovative techniques that connect automated schema design and adaptive indexing with intelligent data partitioning approaches. Generative AI reveals new capabilities in anomaly prevention that escalate system dependability and slash operational interruptions. The framework implements reinforcement learning and transformers to acquire performance database data which subsequently exploits this information to generate outcomes that coincide with adjusting workload patterns. Experimental results beyond cloud database benchmark testing validate higher query execution performance together with strengthened system resource capacity and accelerated performance. The research incorporates ethical considerations along with privacy analysis while making key points to determine suitable AI deployment in cloud environments. Research demonstrates that generative AI changes distributed cloud database procedures by enabling scalable efficient intelligent operational database management systems. This research enhances understanding of AI applications in cloud computing yet serves as a reference for future domain evolution.},
  keywords={Cloud computing;Privacy;Generative AI;Scalability;Query processing;Prevention and mitigation;Distributed databases;Reinforcement learning;Life estimation;Transformers;Generative AI;Distributed Cloud Databases;Scalability;Query Optimization;Intelligent Data Partitioning},
  doi={10.1109/ISBDAS64762.2025.11116985},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10701251,
  author={Anandaputra, Gregorius Valentino and Yungistira, Gianco Armando and Nicholas, Oliver and Mailangkay, Adele B L},
  booktitle={2024 3rd International Conference on Creative Communication and Innovative Technology (ICCIT)}, 
  title={Analysis on User’s Attitude Towards Content Creation Using Artificial Intelligence in Social Media}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={In the digital era, technology, especially Artificial Intelligence (AI), has significantly transformed the creative industry, particularly in content creation. AI enhances efficiency, accelerates production processes, and diversifies creativity in multimedia content development. However, understanding the attitudes of industry practitioners and consumers towards AI adoption in content creation is crucial. This study aims to investigate these attitudes and perceptions using the Technology Acceptance Model (TAM). The research focuses on key factors such as perceived usefulness, perceived ease of use, and their impact on attitudes toward AI-driven content creation. A comprehensive survey was conducted among industry practitioners and consumers to gather insights into their perspectives, concerns, and expectations regarding AI integration in creative practices. The findings indicate a generally positive perception of AI's usefulness in content creation, although concerns about authenticity and control persist. This study provides valuable insights for industry practitioners, decision-makers, and researchers, offering guidance on developing AI applications that are responsive to audience needs and expectations while addressing ethical, safety, and quality considerations. The results underscore the importance of fostering a balanced approach to AI adoption in content creation, ensuring it complements rather than replaces human creativity.},
  keywords={Industries;Surveys;Ethics;Technology acceptance model;Social networking (online);Multimedia systems;Production;Safety;Artificial intelligence;Creativity;Content Creation;Generative AI;Technology Acceptance Model},
