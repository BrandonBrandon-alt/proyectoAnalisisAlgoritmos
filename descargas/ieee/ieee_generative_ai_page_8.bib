  booktitle={2025 3rd International Conference on Intelligent Data Communication Technologies and Internet of Things (IDCIoT)}, 
  title={Deep Learning-Driven Computational Intelligence for Leaf Disease Identification for Smart Agriculture}, 
  year={2025},
  volume={},
  number={},
  pages={1131-1137},
  abstract={In the context of smart farming, Artificial Intelligence (AI) plays a crucial role in enhancing agricultural practices, from soil management to crop yield optimization. By leveraging deep learning and image processing techniques, AI can address various challenges across different agricultural sectors, particularly in detecting and mitigating plant diseases. This paper focuses on the use of these technologies for leaf disease detection to minimize crop loss. Deep learning and image processing are key components in identifying, classifying, and monitoring plant diseases. The study reviews various types of leaf diseases and examines the effectiveness of existing algorithms for detection. To improve performance, an algorithm based on ensemble learning methods is proposed. Experimental investigation is conducted on tomato leaf diseases, followed by a comparative analysis of the results. The proposed AI-driven system enables early disease detection, facilitating timely monitoring and intervention, ultimately reducing crop loss and improving overall crop health through proactive management and precautionary measures.},
  keywords={Smart agriculture;Deep learning;Plant diseases;Reviews;Image processing;Soil;Loss measurement;Classification algorithms;Artificial intelligence;Monitoring;Artificial Intelligence;Deep learning;Smart farming;Image Processing;Pattern Recognition},
  doi={10.1109/IDCIOT64235.2025.10914789},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{11136403,
  author={Sandu, Nitirajsingh and Karim, Shakir and Wangsa, Ketmanto and Singh, Prabhu Jyot},
  booktitle={2025 34th Annual Conference of the European Association for Education in Electrical and Information Engineering (EAEEIE)}, 
  title={Integrating AI Tools in Higher Education: Assessing the Benefits, Risks, and Ethical Implications of an 'AI-First Mindset' in Australian Higher Education}, 
  year={2025},
  volume={},
  number={},
  pages={1-9},
  abstract={The purpose of this research paper is to investigate the incorporation of artificial intelligence (AI) tools into Australian higher education. This study will emphasise the advantages, disadvantages, and ethical repercussions of adopting an "AI-first mindset." This mindset delves into students and faculty, encouraging the use of AI tools for academic research, assessments, and support in educational settings. With the rising use of AI technology by educational institutions to improve learning outcomes, providing personalised student experience and streamline administrative procedures. It is becoming increasingly important to evaluate both the potential benefits and the inherent problems associated with the advent of AI. This research adopts a mixed-methods approach, with quantitative surveys of educators and students used along with qualitative interviews from other stakeholders in the tech industry. The research findings indicate that even though AI tools can improve engagement and efficiency, issues around data privacy, the digital gap, and the possibility of AI bias exist in algorithms. A critical analysis is also performed on the ethical considerations surrounding academic integrity and educators' role in an environment enhanced by artificial intelligence. At the end of the article, recommendations are made for legislative frameworks and best practices that will lead to the appropriate integration of artificial intelligence in higher education. These recommendations will ensure that the advantages are maximised while mitigating the dangers connected with the integration. This research contributes to the changing environment of higher education in the digital era and gives insights to institutions striving to create an ethical and inclusive approach to AI deployment.},
  keywords={Surveys;Industries;Ethics;Data privacy;Technological innovation;Generative AI;Education;Stakeholders;Artificial intelligence;Interviews;AI-powered curriculum;Generative AI;Ethical AI integration;Personalized learning;Australian higher education},
  doi={10.1109/EAEEIE65428.2025.11136403},
  ISSN={2472-7687},
  month={June},}@INPROCEEDINGS{11081382,
