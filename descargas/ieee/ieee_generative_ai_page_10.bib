  abstract={This paper explores the utilization of three prominent AI techniques—Generative Adversarial Networks (GANs), (VAEs), and Recurrent Neural Networks (RNNs)—in SCM. The review provides insights into the application areas, benefits, challenges, and future directions of these AI techniques in optimizing different segments of the supply chain.},
  keywords={Analytical models;Supply chain management;Recurrent neural networks;Uncertainty;Generative AI;Decision making;Supply chains;Demand forecasting;Optimization;Anomaly detection;Generative AI;Generative Adversarial Networks;Variational Autoencoders;Recurrent Neural Networks;Supply Chain Management},
  doi={10.1109/ICOA62581.2024.10753985},
