  author={Rathakrishnan, Mathushaharan and Gayan, Samiru and Edirisinghe, Sampath and Inaltekin, Hazer},
  booktitle={2025 5th International Conference on Advanced Research in Computing (ICARC)}, 
  title={A Multi-Model Framework for Synthesizing High-Fidelity Network Intrusion Data Using Generative AI}, 
  year={2025},
