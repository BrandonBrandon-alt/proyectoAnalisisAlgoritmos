  journal={IEEE P3129/D3, August 2022}, 
  title={IEEE Approved Draft Standard for Robustness Testing and Evaluation of Artificial Intelligence (AI)-based Image Recognition Service}, 
  year={2023},
  volume={},
  number={},
  pages={1-30},
  abstract={Test specifications with a set of indicators for common corruption and adversarial attacks, which can be used to evaluate the robustness of artificial intelligence-based image recognition services are provided in this standard. Robustness attack threats and establishes an assessment framework to evaluate the robustness of artificial intelligence-based image recognition service under various settings are also specified in this standard.},
  keywords={IEEE Standards;Artificial intelligence;Image recognition;Robustness;Generative adversarial networks;adversarial attacks;artificial Intelligence-based services;assessment framework;common corruption;IEEE 3129;robustness},
  doi={},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10695632,
  author={Lim, Tiong Hoo and Muhammad, Dk Norhafizah Pg Hj and Najirahmat, Nur Mazira Nasirah},
  booktitle={2024 Asia-Pacific Conference on Image Processing, Electronics and Computers (IPEC)}, 
  title={Artificial Intelligence based Best-First Decision Table for Gesture Unit Segmentation}, 
  year={2024},
  volume={},
  number={},
  pages={641-645},
  abstract={Gesture analysis has emerged as a prominent method in human-computer interaction innovation. Its evolution has extended its utility beyond traditional boundaries, notably in automating tasks associated with gesture phases segmentation for disclosure analysis. This study introduces an AI-driven Best- First Decision Table for gesture detection, compared against four distinct classification algorithms using the Weka data mining tool. A K-Star algorithm is also applied and compared to analyse the detection of gesture which is never been analysed in other papers. A data set of 20 attributes is used where the classes are divided into 5. The experimental results are analysed and compared against MultiLayer Perceptron (MLP) classification. The simulated results have shown that the proposed Best-First Decision Table and K-Stars have achieved a higher True Positive Rate than MLP with a higher F-Score without affecting the False Positive Rate.},
  keywords={Human computer interaction;Technological innovation;Accuracy;Image processing;Multilayer perceptrons;Classification algorithms;Decision trees;Artificial intelligence;Videos;Matlab;machine language;decision tree;classification;gesture analysis},
  doi={10.1109/IPEC61310.2024.00116},
  ISSN={},
  month={April},}@ARTICLE{10539953,
  author={Ward, Rory and Bigioi, Dan and Basak, Shubhajit and Breslin, John G. and Corcoran, Peter},
  journal={IEEE Access}, 
  title={LatentColorization: Latent Diffusion-Based Speaker Video Colorization}, 
  year={2024},
  volume={12},
  number={},
  pages={81105-81121},
  abstract={While current research predominantly focuses on image-based colorization, the domain of video-based colorization remains relatively unexplored. Many existing video colorization techniques operate frame-by-frame, often overlooking the critical aspect of temporal coherence between successive frames. This approach can result in inconsistencies across frames, leading to undesirable effects like flickering or abrupt color transitions between frames. To address these challenges, we combine the generative capabilities of a fine-tuned latent diffusion model with an autoregressive conditioning mechanism to ensure temporal consistency in automatic speaker video colorization. We demonstrate strong improvements on established quality metrics compared to existing methods, namely, PSNR, SSIM, FID, FVD, NIQE and BRISQUE. Specifically, we achieve an 18% improvement in performance when FVD is employed as the evaluation metric. Furthermore, we performed a subjective study, where users preferred LatentColorization to the existing state-of-the-art DeOldify 80% of the time. Our dataset combines conventional datasets and videos from television/movies. A short demonstration of our results can be seen in some example videos available at https://youtu.be/vDbzsZdFuxM.},
  keywords={Streaming media;Training;Image color analysis;Generative adversarial networks;Diffusion processes;Benchmark testing;Task analysis;Artificial intelligence;Artificial neural networks;Computer vision;Artificial intelligence;artificial neural networks;machine learning;computer vision;video colorization;latent diffusion;image colorization},
  doi={10.1109/ACCESS.2024.3406249},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{11158547,
  author={Liang, Zhaoying and Zhang, Fangye and Zhou, Tiange},
  booktitle={2025 5th International Conference on Artificial Intelligence and Education (ICAIE)}, 
  title={Generative AI in LEGO Education: Opportunities & Ethical Challenges in Digital Learning Development}, 
  year={2025},
  volume={},
  number={},
  pages={779-784},
  abstract={Generative AI offers innovative opportunities for the digital development of LEGO education and poses the dual challenges of technological ethics and the nature of education. This study explores the core application scenarios of generative AI in the development of LEGO digital learning resources, which significantly improves the efficiency and creative boundaries of educational resource development. Generative AI significantly improves the development efficiency and creative boundaries of digital learning resources by rapidly generating LEGO model prototypes and assisting interdisciplinary curriculum design. At the same time, it dynamically adjusts task difficulty and provides real-time feedback by analyzing student behavioral data, enabling truly personalized learning support. These applications lower the threshold of innovation, expand educational possibilities, and promote the construction of a sustainable education ecosystem by optimizing the reuse of resources. However, this convergence also poses key techno-ethical risks, including data bias, privacy concerns arising from excessive data collection from children, and homogenization of creativity due to AI “best solution” algorithms. To address this issue, we propose a two-track evaluation mechanism for “ethical education” that defines the boundaries of human-computer collaboration, and emphasizes the need for shared social responsibility to ensure positive empowerment in the generation of AI. Future research directions include deepening multi-sensory learning experiences that combine reality with reality, and further exploring synergistic development paths between technology and human-centered education.},
  keywords={Ethics;Technological innovation;Data privacy;Generative AI;Education;Ecosystems;Prototypes;Learning (artificial intelligence);Data collection;Real-time systems;Generative Artificial Intelligence;Lego Education;Digital Learning Resources;Educational Ethics;Personal-ized Learning;Inter-disciplinary Integration;Creative Development},
  doi={10.1109/ICAIE64856.2025.11158547},
  ISSN={},
  month={May},}@ARTICLE{10423763,
  author={Fang, Fulai and Jiang, Xiaohong},
  journal={IEEE Access}, 
  title={The Analysis of Artificial Intelligence Digital Technology in Art Education Under the Internet of Things}, 
  year={2024},
  volume={12},
  number={},
  pages={22928-22937},
  abstract={This study aims to explore the integration of Internet of Things (IoT) technology and artificial intelligence (AI) in art education, assessing its impact on learners’ experiences and learning outcomes. The study first proposes a digital teaching system that enables the IoT and Generative Adversarial Networks (GANs) to play a role in art education by monitoring students’ creative state in real-time, providing immediate feedback, and facilitating the generation of creative works. The system framework includes sensor nodes, an IoT platform, a GAN model, and a user interface to build a real-time interactive environment. Sensor nodes constantly collect physiological, movement, and environmental data from students, and the GAN model receives student data from the IoT platform, combining creative input from students to generate artwork in real-time. The generated works are transmitted to the discriminator through the IoT platform, which evaluates their quality and provides real-time feedback. Students interact with the system through the user interface, observe the generated artwork, adjust generator parameters, and propose new ideas. These interactions influence further artistic creation. The WikiArt public art creation dataset is selected to establish the experimental foundation, and the experimental evaluation focuses on image generation quality, system performance, and student learning outcomes. It is compared with Deep Convolutional Generative Adversarial Network (DCGAN) and Variational Autoencoder (VAE) models. The research results indicate that the designed IoT and GANs integrated system remarkably outperforms DCGAN and VAE in image generation quality, with an Inception Score of 4.5, which is more diverse and recognizable than other models. Regarding system performance, the IoT and GANs integrated system is significantly ahead in image generation speed and user interaction, with a transmission speed of up to 200 Mbps. Regarding student learning outcomes, the system performs excellently in emotional feedback, learning outcomes, and creative work quality, achieving 80% satisfaction and 90% positive feedback. Overall, the research conclusion clearly points out that the integration of IoT and GANs has a significant and comprehensive effect on improving the quality of art education. This study expands the field of art education by integrating IoT and GANs, enhancing students’ creative experiences, and providing innovative methods for art teaching in the digital age.},
  keywords={Education;Art;Artificial intelligence;Real-time systems;Generative adversarial networks;Internet of Things;Image synthesis;Electronic learning;Internet of Things technology;generative adversarial network;art education;digital teaching},
  doi={10.1109/ACCESS.2024.3363655},
  ISSN={2169-3536},
  month={},}@ARTICLE{10883968,
  author={Chen, Zhuowen and Sarkis, Joseph and Yildizbasi, Abdullah},
  journal={IEEE Engineering Management Review}, 
  title={Integrating Generative AI into Circular Supply Chain Safety Management: A Forward-Looking Perspective}, 
  year={2025},
  volume={},
  number={},
  pages={1-17},
  abstract={Circular supply chains (CSCs) aim to minimize ecological footprints by closing the loop on resource use. However, safety concerns in CSCs, especially in activities like recycling and remanufacturing, present significant barriers. Current safety management systems in CSCs are often reactive and limited in scope, failing to address safety concerns in inter-organizational complexities. Therefore, we explore how generative artificial intelligence (Gen AI) can revolutionize safety management in CSCs by providing proactive monitoring, predictive analysis, and enhanced training. Nevertheless, integrating this technology requires addressing technical, data security, operational, trust, and ethical issues. Future research and practices should focus on overcoming these barriers to harness Gen AI's potential for enhancing safety in CSCs.},
  keywords={Safety;Safety management;Artificial intelligence;Supply chains;Training;Organizations;Generative AI;Sustainable development;Recycling;Software;Generative artificial intelligence;Circular economy;Health;Safety;Supply Chain;Sustainability},
  doi={10.1109/EMR.2025.3541542},
  ISSN={1937-4178},
  month={},}@INPROCEEDINGS{11101876,
  author={Wu, Jielin},
  booktitle={2025 9th International Symposium on Innovative Approaches in Smart Technologies (ISAS)}, 
  title={Joint Application of Transfer Learning and Generative Adversarial Networks in Addressing the Long-Tail Problem}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={In recent years, the deep learning, and artificial intelligence, have advanced rapidly and make significant breakthroughs with the help of generative architectures. In general, these are broadly broken down into either Text to image (T2I) architectures or Large Language Models (LLMs). Generating AI has been broadly explored in most domains, but the detection of long tailed is undeveloped. The scarcity of data for tail classes becomes the key challenge among its shortage in long-tailed image classification, and thus poor generalization tends to occur. To address this issue, traditional methods make use of constructive learning, mix based augmentation, re weighting, and resampling. But still, all these techniques do not generalize well to the representations of tail classes for data imbalance. In order to tackle the limitations of it, this research develops a hybrid generative learning model with synthetic data augmentation, contrastive learning, and adaptive label correction for improving long tailed image detection. The architecture takes advantage of T2I models to create synthetic data that is then refined via mix-based augmentation (MixUP and CutMix) and processed through Contrastive learning base encoder. The meaningful representations are extracted using a ResNet backbone and the model generalization is improved using supervised contrastive learning (SCL). Moreover, there is a label correction module which dynamically identifies low quality synthetic labels using KNN method, providing data integrity. The proposed method is also demonstrated to significantly improve the classification accuracy for the tail classes, and to outperform conventional techniques. Generative AI is shown to promise to fill the gap between synthetically and real-life data and make robust long tailed image classification possible with an adaptive and correcting nature.},
  keywords={Adaptation models;Heavily-tailed distribution;Generative AI;Telecommunication traffic;Contrastive learning;Nearest neighbor methods;Feature extraction;Communication switching;Synthetic data;Image classification;Long-tailed Image Classification;Generative AI;Contrastive Learning;Synthetic Data Augmentation;ResNet-based Feature Extraction},
  doi={10.1109/ISAS66241.2025.11101876},
  ISSN={},
  month={June},}@INPROCEEDINGS{10764687,
  author={Nisiotis, Louis},
  booktitle={2024 IEEE International Symposium on Emerging Metaverse (ISEMV)}, 
  title={A System Architecture for AI-Driven Interactive Systems Within a CPSS Framework for Metaverse Applications}, 
  year={2024},
  volume={},
  number={},
  pages={9-12},
  abstract={This paper proposes a system architecture for deploying AI-driven interactive systems within the context of a Cyber-Physical-Social System framework, specifically designed for applications in the emerging Metaverse, applicable to a plethora of domains. The proposed architecture integrates advanced technologies to facilitate immersive user experiences and intelligent system interactions based on user actions and real-time data inputs from the real and the digital world(s), digital twins, artificial agents and elements. The integration of the proposed architecture components aim to support sophisticated interactions between physical, digital entities, and humans within the Metaverse. The paper presents the theoretical underpinnings and system description of the proposed architecture, contributing to future developments in Metaverse systems and technologies.},
  keywords={Metaverse;Generative AI;Extended reality;Interactive systems;Systems architecture;Real-time systems;Digital twins;Intelligent systems;Metaverse;eXtended Reality;Artificial Intelligence;Cyber-Physical-Social Systems;Generative AI},
  doi={10.1109/ISEMV63338.2024.00016},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10833961,
  author={Schimanke, Florian},
  booktitle={2024 Artificial Intelligence x Humanities, Education, and Art (AIxHEART)}, 
  title={Media Competence is the key requirement when using Generative AI in Academic Education in a meaningful way}, 
  year={2024},
  volume={},
  number={},
  pages={46-47},
  abstract={With the release of ChatGPT, Pandora’s box of generative AI has been opened and the new technology is here to stay. This also impacts academic education, where it provides new opportunities while also bearing certain challenges. Students and educators alike will have to adapt to the newly available technology and find ways to use it in a meaningful and profitable way. Summarized, this means that both parties need to develop media competence in using generative AI on their end in academic education. New ways of learning and teaching are required, as well as a change in how educators grade the students’ learning status. While the latter should be made aware of the opportunities that generative AI provides them with, educators should actively make use of it in their courses in order to enhance their teaching.},
  keywords={Hands;Art;Generative AI;Education;Media;Chatbots;artificial intelligence;AI;education;ChatGPT;generative AI;plagiarism},
  doi={10.1109/AIxHeart62327.2024.00015},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10125121,
  author={Qadir, Junaid},
  booktitle={2023 IEEE Global Engineering Education Conference (EDUCON)}, 
  title={Engineering Education in the Era of ChatGPT: Promise and Pitfalls of Generative AI for Education}, 
  year={2023},
  volume={},
  number={},
