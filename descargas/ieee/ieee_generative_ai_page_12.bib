@INPROCEEDINGS{10871519,
  author={Xie, Qing and Zhang, Yanyan},
  booktitle={2024 International Conference on Intelligent Computing and Next Generation Networks (ICNGN)}, 
  title={Research on the Interpretability and Controllability of Deep Learning-Based Exhibition Design Style Generation and Transfer}, 
  year={2024},
  volume={},
  number={},
  pages={01-05},
  abstract={With the rapid development of deep learning technology, Generative Adversarial Networks (GANs) have achieved significant results in the field of image generation. However, traditional GAN models are often seen as “black boxes,” with their internal decision-making processes lacking transparency. This limits their trustworthiness and controllability in practical applications. This study addresses the needs for generating and transferring exhibition hall design styles by proposing a deep learning model that combines Explainable Artificial Intelligence (XAI) technology and a user preference adaptation system. By introducing an attention mechanism to design interpretability modules, the transparency of the generation process is enhanced. A personalized user input interface is developed, enabling controllability and flexibility in style generation. Moreover, a multimodal information fusion method is employed to improve the accuracy of text-to-image generation. The experimental results show that the improved model not only enhances user trust and satisfaction with the generated results but also demonstrates high accuracy and diversity in the tasks of generating and transferring exhibition hall design styles.},
  keywords={Deep learning;Accuracy;Explainable AI;Text to image;Process control;Generative adversarial networks;Controllability;User preference;Optimization;Next generation networking;Deep learning;generative adversarial networks;explainable artificial intelligence;exhibition design;style generation;controllability},
  doi={10.1109/ICNGN63705.2024.10871519},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{11115325,
  author={J, Janisha and Joseph, Nalini},
  booktitle={2025 12th International Conference on Computing for Sustainable Global Development (INDIACom)}, 
  title={Advanced Artificial Intelligence Encryption Method for Secure Transfer of Medical Images}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={With the advancement of medical technology, it is now crucial to diagnose illnesses using medical imaging. From one end of the network to the other, medical pictures frequently travel across its branches. High-level security is therefore necessary. Unauthorised usage of the image's data causes issues. One of the most efficient approaches in this area is encryption, which is one of the strategies used to protect the data in the picture. Here, we focus on two key steps: confusion and dispersion. The deep neural network is modified by the amount of weight that has the most impact on the output, whether it be an intermediate result or an elimination output, in addition to an unorganised structure that is undetected utilising deep neural network techniques. The proposed method used colour and greyscale images by segmenting them according to the Region of Interest of the deep neural network algorithm. In order to randomly construct an unpredictable structure that involved the addition or deletion of columns and as well as the rows, the method was then utilised to randomly distribute the pixels inside the designated region and generate random integers. To demonstrate the value of the suggested approach, the suggested algorithm is assessed in a number of ways and contrasted with the current approaches. The model number of changing pixel rate (NPCR) provides a 99.62 value.},
  keywords={Image segmentation;Image color analysis;Artificial neural networks;Encryption;Security;Artificial intelligence;Medical diagnostic imaging;Dispersion;Neural Network;Segmentation;Encryption;Medical Image;Region of Interest},
  doi={10.23919/INDIACom66777.2025.11115325},
  ISSN={},
  month={April},}@INPROCEEDINGS{9687901,
  author={Zhao, Hang and Liu, Xuebin and Wang, Shuang},
  booktitle={2021 IEEE 2nd International Conference on Information Technology, Big Data and Artificial Intelligence (ICIBA)}, 
  title={Using Landsat-8 imagery and Generative Adversarial Network for Glacial Lakes Mapping in High Mountain Regions}, 
  year={2021},
  volume={2},
  number={},
  pages={877-881},
  abstract={As an essential resource in High Mountain Regions (HMR), evaluating the glacial lake dynamics is of great significance to explore the impacts of climate changes and predict the risks of Glacial Lake Outburst Floods (GLOFs). However, complicated and laborious methods for glacial lake mapping are unpractically applied in automatically monitoring glacial lakes at a large-scale region. In this work, we explored the mapping efficiency of glacial lakes by combing Generative Adversarial Networks and multi-level feature pyramid (GANMFP) in HMR. We first sampled the image patches containing glacial lakes from Landsat-8 raw data to evaluate the model performance. Totally, 6583 patches with 256x256x7 pixels are randomly cropped from 62 Landsat-8 images. Then we employed these data to train and test the GAN model, which integrated a multi-level feature fusion module in generator and a Resnet-152 networks in discriminator. From the validation results and result visualization, our method achieved good performances in Precision (88.42), Recall (59.61), and Overall Accuracy (99.28), which shows excellent potential in glacial lake mapping at a large-scale region.},
  keywords={Earth;Artificial satellites;Data visualization;Lakes;Generative adversarial networks;Generators;Data models;Climate change;Glacial Lake;High Mountain Regions;Generative Adversarial Networks;Landsat-8 OLI},
  doi={10.1109/ICIBA52610.2021.9687901},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{11091888,
  author={Chen, Yujie and Zhang, Qi and Wang, Zixuan and Zhang, Jin},
  booktitle={2025 7th International Conference on Computer Science and Technologies in Education (CSTE)}, 
  title={Enhancing Pre-Service Teachers' Design Skills with Generative AI: An Empirical ICAP Study}, 
  year={2025},
  volume={},
  number={},
  pages={164-167},
  abstract={Generative Artificial Intelligence (AI) has seen increasing application in the field of education, particularly in enhancing pre-service teachers' instructional design abilities, demonstrating significant potential. Drawing on the ICAP framework theory, this paper conducts an empirical study using the generative AI tool Wenxin Yiyan. This study involved 32 pre-service teachers majoring in Educational Technology, each submitting an instructional design for basic IT instruction in elementary schools, both before and after the application of generative AI. After the experiment, a questionnaire survey was conducted to obtain feedback from pre-service teachers on their use of generative AI. The collected data were analyzed using paired T-tests and cognitive network analysis. The results showed that generative AI assistance significantly improved the quality of teaching design plans and enhanced the teaching design capabilities of pre-service teachers. Moreover, pre-service teachers of different performance levels demonstrated distinct cognitive network characteristics when using generative AI assistance.},
  keywords={Training;Surveys;Computer science;Generative AI;Education;Curriculum development;Network analyzers;Educational technology;generative AI;pre-service teachers;ICAP framework;instructional design;cognitive network analysis},
  doi={10.1109/CSTE64638.2025.11091888},
  ISSN={},
  month={April},}@INPROCEEDINGS{11170704,
  author={Thambireddy, Sankar and Reddy Bussu, Venkata Ramana and Madathala, Harikrishna and Mane, Vijay and Inamdar, Chinmay},
  booktitle={2025 5th International Conference on Soft Computing for Security Applications (ICSCSA)}, 
  title={AI-Enabled SAP Enterprise Systems: A Comprehensive Business Use Case Survey}, 
  year={2025},
  volume={},
  number={},
  pages={1045-1052},
  abstract={The integration of artificial intelligence (AI) technologies into enterprise resource planning (ERP) systems represents a critical transformation in modern business operations, yet comprehensive analysis of AI implementation across SAP’s enterprise ecosystem remains fragmented. This paper presents a systematic survey of AI use cases and implementation frameworks within SAP business applications, examining the current state of AI-enabled process optimization across enterprise domains. Our methodology encompasses a structured analysis of SAP’s Business AI portfolio, evaluating AI capabilities across three primary business domains: Finance, Supply Chain, and Customer Experience. We examine the technical architecture of SAP’s AI foundation, including the Joule copilot framework, SAP Business Technology Platform (BTP) integration, and the company’s responsible AI governance structure. Key findings reveal 25+ specific AI use cases spanning from machine learning-driven demand forecasting to generative AI-powered content creation, with particular emphasis on process automation in tax compliance, visual inspection, and intelligent sales execution. The analysis identifies SAP’s multi-layered AI approach incorporating foundation models, large language models, and domain-specific algorithms integrated through a unified platform architecture. These findings demonstrate significant implications for enterprise digital transformation strategies, highlighting the evolution from traditional ERP systems to AI-native business platforms that enhance decision-making capabilities and operational efficiency across critical business processes.},
  keywords={Surveys;Ethics;Systematics;Supply chains;Finance;Organizations;Computer architecture;User experience;Artificial intelligence;Optimization;Artificial Intelligence;Enterprise Resource Planning;SAP Business AI;Machine Learning;Generative AI;Business Process Optimization},
  doi={10.1109/ICSCSA66339.2025.11170704},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{10116629,
  author={Mouli, D v v v Ch.},
  booktitle={2023 10th International Conference on Signal Processing and Integrated Networks (SPIN)}, 
  title={Artificial Intelligence Enabled Framework for Automatic Brain Stroke Detection}, 
  year={2023},
  volume={},
  number={},
  pages={659-664},
  abstract={Brain stroke is causing severe damage to health besides causingdeaths across the globe. Healthcare industry has got technological innovations to improve quality of service. With the emergence of Artificial Intelligence (AI) medical image analysis has unprecedented possibilities. Deep learning approaches have gained popularity for analysing medical images for automatic diagnosis. Convolutional Neural Network (CNN) based deep learning models such as VGG16, ResNet50 and DenseNet121 are found to be suitable for image processing. However, as one size does not fit all, there is need for optimizing these models in order to adapt them for brain stroke detection and leverage their performance. Towards this end, in this paper, we proposed an AI enabled framework with optimizations to the aforementioned deep models. Towards this end we proposed an algorithm known as Intelligent Learning for Stroke Detection (ILSD). The optimized models are found to have increased prediction performance. Enhanced DenseNet121 showed highest accuracy with 99.82%.},
  keywords={Deep learning;Adaptation models;Technological innovation;Signal processing algorithms;Learning (artificial intelligence);Signal processing;Brain modeling;Convolutional neural networks;Medical diagnostic imaging;Residual neural networks;Brain stroke detection;artificial intelligence;deep learning models;deep learning enhancement x},
  doi={10.1109/SPIN57001.2023.10116629},
  ISSN={2688-769X},
  month={March},}@INPROCEEDINGS{11070779,
  author={Jawar, Sangeeta and Kashyap, Ramgopal},
  booktitle={2025 4th OPJU International Technology Conference (OTCON) on Smart Computing for Innovation and Advancement in Industry 5.0}, 
  title={Artificial Intelligence Based Automated Medical Imaging Analysis and Interpretation}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Adding artificial intelligence (AI) to medical imaging has changed how diagnoses are made by making it possible to look at images, especially chest X-rays, more accurately and automatically. This study investigates how advanced AI techniques, such as deep learning and neural networks, can automate the assessment of the cardiothoracic ratio, a crucial indicator for identifying heart diseases. AI uses huge records and complicated formulas to make medical findings better, faster, and more consistent, so humans don't have to look them over as often. The study also examines the challenges that arise from combining AI, such as inaccurate data and errors, and emphasizes the importance of conducting comprehensive reviews to evaluate the effectiveness of these AI-powered solutions. This study shows how AI could greatly improve patient results by helping to find and diagnose cardiovascular diseases earlier using computerized medical image analysis.},
  keywords={Deep learning;Technological innovation;Accuracy;Image analysis;Neural networks;Medical services;Artificial intelligence;Medical diagnostic imaging;X-ray imaging;Testing;Artificial Intelligence;Automated Diagnosis;Cardiothoracic Ratio;Chest Radiography;Deep Learning;Image Analysis;Medical Imaging;Neural Networks;Radiology;Systematic Review},
  doi={10.1109/OTCON65728.2025.11070779},
  ISSN={},
  month={April},}@INPROCEEDINGS{11158382,
  author={Longlong, Chen and Shoujun, Qiao and Lixiang, Gao and Xinbing, Luo and Gang, Chen and Zaiping, Liu},
  booktitle={2025 5th International Conference on Artificial Intelligence and Education (ICAIE)}, 
  title={Research on Generative Artificial Intelligence Promoting Interdisciplinary Deep Learning Mechanisms and Learning and Teaching Strategies}, 
  year={2025},
  volume={},
  number={},
  pages={429-434},
  abstract={With the development of science and technology, Generative Artificial Intelligence(GAI) has become a key technology to promote social progress and provides impetus for the development of education. In the new era, it is necessary to cultivate innovative and compound talents to adapt to the development of future society. Interdisciplinary deep learning provides a way for the cultivation of these talents. Interdisciplinary deep learning is a complex learning process of discipline integration, and GAI provides support for it with its unique technology. As a tool, GAI, when dealing with this new learning mode of interdisciplinary deep learning, needs to find the mechanism of the two and form the strategies of interdisciplinary deep learning based on GAI, so that GAI technology can be truly applied to the practice of interdisciplinary deep learning, facilitating the in-depth comprehensive development of individuals and the cultivation of innovative talents.},
  keywords={Deep learning;Generative AI;Education;Learning (artificial intelligence);Compounds;GAI;Interdisciplinary Learning;Deep Learning;Internal Mechanisms;Learning and Teaching Strategies},
  doi={10.1109/ICAIE64856.2025.11158382},
  ISSN={},
  month={May},}@INPROCEEDINGS{10653119,
  author={Kongthon, Alisa and Supnithi, Pornchai},
  booktitle={2024 Portland International Conference on Management of Engineering and Technology (PICMET)}, 
  title={Artificial Intelligence Applications in Ionospheric Irregularities: A Bibliometric Analysis}, 
  year={2024},
  volume={},
  number={},
  pages={1-6},
  abstract={Ionospheric irregularities such as equatorial plasma bubbles (EPBs) in low-latitude regions often lead to disruptions in trans-ionospheric radio systems, navigation systems and satellite communications. Understanding and monitoring equatorial plasma bubbles is important for improving the reliability of communication and navigation systems. Recently, artificial intelligence (AI) has been applied to a wide variety of satellite communication aspects including ionospheric irregularities detection. This paper aims to apply bibliometric analysis on research publications related to AI applications in ionospheric irregularities. Such analysis can help researchers understand the evolving trends in AI and its diverse sub-fields, guiding future research directions. In addition, researchers can use the results of bibliometric analysis to benchmark their own work against the broader research landscape, identifying areas where their contributions can have the most impact.},
  keywords={Support vector machines;Bibliometrics;Radio navigation;Predictive models;Satellite navigation systems;Market research;Plasmas},
  doi={10.23919/PICMET64035.2024.10653119},
  ISSN={2159-5100},
  month={Aug},}@INPROCEEDINGS{11208129,
  author={Leong, Wai Yie},
  booktitle={2025 IEEE International Conference on Consumer Electronics - Taiwan (ICCE-Taiwan)}, 
  title={Generative AI-Driven Digital Fabrication: Revolutionizing 3D Printing with AI-Designed Materials}, 
  year={2025},
  volume={},
  number={},
  pages={45-46},
  abstract={Generative AI is revolutionizing digital fabrication and additive manufacturing by optimizing material properties, enhancing design precision, and streamlining production efficiency. This paper explores the evolution of AI-driven digital fabrication, from its historical development to its current applications in 3D printing. A review highlights AI’s role in material selection, defect prediction, and process optimization. The methodology of generative AI models such as GANs and reinforcement learning to develop novel material compositions with superior mechanical properties. A case study in aerospace component manufacturing demonstrates AI-optimized materials achieving enhanced strength, reduced weight, and lower production costs compared to traditional methods. Challenges such as computational demands, data biases, and regulatory compliance are discussed alongside future research directions, including real-time defect prediction and sustainable AI-driven material recycling. This study underscores the transformative impact of generative AI in digital fabrication, paving the way for next-generation manufacturing solutions.},
  keywords={Fabrication;Industries;Technological innovation;Generative AI;Computational modeling;Production;Three-dimensional printing;Optimization;Next generation networking;Digital information;Artificial Intelligence (AI);additive manufacturing;digital fabrication},
  doi={10.1109/ICCE-Taiwan66881.2025.11208129},
  ISSN={2575-8284},
  month={July},}@ARTICLE{9900292,
