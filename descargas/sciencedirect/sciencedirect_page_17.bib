@article{BANKINS2024101828,
title = {Artificial intelligence, workers, and future of work skills},
journal = {Current Opinion in Psychology},
volume = {58},
pages = {101828},
year = {2024},
issn = {2352-250X},
doi = {https://doi.org/10.1016/j.copsyc.2024.101828},
url = {https://www.sciencedirect.com/science/article/pii/S2352250X24000411},
author = {Sarah Bankins and Xinyu Hu and Yunyun Yuan},
keywords = {Artificial intelligence, AI and work, Future of work, AI and skills, AI literacy},
abstract = {Historically, the use of technology in organizations has reshaped the nature of human work. In this article, we overview how current waves of artificially intelligent (AI) technologies are following this trend, showing how its uses can both automate and complement human labor, alongside creating new forms of human work. However, AI can also generate both upsides and downsides for workers' experiences, which are dependent upon a range of factors such as how the technology is used and the support employees receive during digital transitions. We conclude by outlining how AI literacy and other human-centered skills will play an increasingly important role in future workplaces.}
}
@article{VERMA2025110937,
title = {DynLeafNet: A dynamic lightweight architecture for plant disease classification using dynamic residual network with explainable artificial intelligence},
journal = {Engineering Applications of Artificial Intelligence},
volume = {155},
pages = {110937},
year = {2025},
issn = {0952-1976},
doi = {https://doi.org/10.1016/j.engappai.2025.110937},
url = {https://www.sciencedirect.com/science/article/pii/S0952197625009376},
author = {Preeti Raj Verma and Deepika Pantola and Navneet Pratap Singh},
keywords = {Deep learning, Plant disease detection, Explainable artificial intelligence, Early exiting, Learned group convolutions, CondenseNet},
abstract = {Agriculture is a vital sector contributing to food security and economic growth, but it faces significant hurdles in food production due to declining crop yields, with plant diseases being a key factor. Moreover, early detection of plant diseases remains a major challenge due to class imbalance in datasets and the difficulty in identifying initial symptoms. To tackle these challenges, this study proposes a novel lightweight dynamic model called DynLeafNet, a deep learning-based architecture specifically designed for efficient and accurate recognition of plant disease. DynLeafNet leverages residual Convolutional Neural Networks (CNNs) enhanced with advanced dynamic techniques such as Learned Group Convolutions (LGConv), Early Exiting, and Grid Search to optimize performance and computational efficiency. Moreover, the proposed model uses data augmentation to address the Class Imbalance Problem (CIP) by calculating class weights in the datasets. The efficacy of the DynLeafNet model is validated using the publicly available PlantVillage and Rice Leaf Disease datasets, where it demonstrated superior accuracy and reduced computational cost (calculated in terms of Floating Point Operations per Second (FLOPs)) compared to existing state-of-the-art models. For the PlantVillage dataset, DynLeafNet achieved a training accuracy of 99.34%, a testing accuracy of 98.33%, and a computational complexity of 17.64M FLOPs. For the Rice Leaf Disease dataset, the model achieved a training accuracy of 97.81%, a testing accuracy of 95.64%, and a computational complexity of 35.07M FLOPs. In addition, single image prediction is performed using Gradient Weighted Class Activation Mapping ++ (Grad-CAM ++), to visualize the model’s focus areas and enhance interpretability.}
}
@article{BONAPARTE2024104851,
