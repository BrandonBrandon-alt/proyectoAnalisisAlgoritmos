@article{DOMANIC2024S25,
title = {Evaluating student responses from the perspectives of artificial intelligence},
journal = {International Dental Journal},
volume = {74},
pages = {S25},
year = {2024},
note = {Abstracts of the 2024 FDI World Dental Congress (Special Edition)},
issn = {0020-6539},
doi = {https://doi.org/10.1016/j.identj.2024.07.644},
url = {https://www.sciencedirect.com/science/article/pii/S0020653924008360},
author = {Kubra Yildiz Domanic and Şükran Baycan},
abstract = {AIM or PURPOSE
Chat Generative Pre-Trained Transformer (ChatGPT) is an open artificial intelligence (AI)-powered chatbot with various academic dentistry applications, including dental prosthetic technology (DPT) and oral health program (OH). Since its release, ChatGPT and Gemini has taken the world by storm with its utilization in various fields of life. The main goal of this review was to thoroughly and factually assess the potential of ChatGPT and Gemini as a tool for researching exam answers for students studying in the field of dental prosthesis technology and oral health. This could guide subsequent research.
MATERIALS and METHOD
This was an exploratory study investigating the accuracy of ChatGPT and Gemini to attempt a range of recognized assessments in undergraduate dental prosthetic technology and oral health curricula. ChatGPT and Gemini were used to attempt ten items based on each of the three commonly used question formats including multiple-choice questions (MCQs); short answer questions (SAQs); True/False questions.
RESULTS
ChatGPT and Gemini applications provided accurate responses to majority of knowledge-based assessments based on MCQs, SAQs, True/False items. Responses generated to written assignments were also satisfactory apart from those for critical appraisal of literature.
CONCLUSION(S)
Notwithstanding their current limitations, generative AI-based applications have the potential to revolutionize virtual learning. Instead of treating it as a threat, dental prosthetic technology and oral health educators need to adapt teaching and assessments in dental prosthetic technology and oral health education to the benefits of the learners whilst mitigating against dishonest use of AI-based applications.}
}
