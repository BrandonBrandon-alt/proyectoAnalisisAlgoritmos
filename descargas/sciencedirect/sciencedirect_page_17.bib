@article{BHATTACHARJEE2025100027,
title = {Artificial intelligence and machine learning for the optimization of photocatalytic performance},
journal = {Energy 360},
volume = {3},
pages = {100027},
year = {2025},
issn = {2950-4872},
doi = {https://doi.org/10.1016/j.energ.2025.100027},
url = {https://www.sciencedirect.com/science/article/pii/S2950487225000145},
author = {Jyoti Bhattacharjee and Subhasis Roy and Abdul Aziz Shaikh},
keywords = {Hydrogen generation, Photocatalysts, Reaction kinetics, AI, Reactors, Sustainability},
abstract = {In the quest for sustainable energy solutions, photocatalysts for hydrogen production have emerged as front-runner innovations that offer a promising route to clean and renewable energy. This review article brings the recent advances in the field of photocatalysts for hydrogen (H₂) production up to date concerning the criteria for choosing photocatalysts and the modifications that can be made on a photocatalyst to adjust its photocatalytic activity. The production of hydrogen is being brought forward as an upcoming clean energy source, and semiconductor materials are revealed to have an essential function in this field of application. Different approaches for enhancing the photocatalytic activities of semiconductor materials, including mechanisms and thermodynamic aspects, are addressed in this paper. There is a discourse on using reinforcement learning algorithms in photocatalysis and dynamic optimization of reaction conditions towards colossal efficiency and flexibility in sustainable environmental processes. The majority of semiconductor photocatalysts employed in this study include titanium oxide, g-C₃N₄, graphene, sulfides, oxides, nitrides, oxysulfides, and oxynitrides; cocatalyst-based materials seek to enhance their photocatalytic performance under visible light irradiation. This article describes improved compatibility between metals and non-metals during the construction of heterojunctions, such as type I–III structures; the sophisticated systems referred to as Z-scheme and S-scheme structures; and the function of mediators in the indirect construction of a Z-scheme assembly through suitably aligned bands, which improves the photocatalytic activity. In addition, it demonstrates the most critical parameters for maximizing photocatalytic efficiency, such as reaction chemistry, mass transfer, reaction kinetics, limits to light diffusion, and selecting proper light sources and photoreceptors. Finally, this review ends with an overview of how these developments, combined with artificial intelligence (AI) algorithms, complete highly efficient and sustainable hydrogen production technologies, offering a road map for additional research and development in this field.}
}
@article{YU2025104855,
title = {Research progress on the artificial intelligence applications in food safety and quality management},
journal = {Trends in Food Science & Technology},
volume = {156},
pages = {104855},
year = {2025},
issn = {0924-2244},
doi = {https://doi.org/10.1016/j.tifs.2024.104855},
url = {https://www.sciencedirect.com/science/article/pii/S0924224424005314},
author = {Wenbin Yu and Zhiwei Ouyang and Yufei Zhang and Yi Lu and Changhe Wei and Yayi Tu and Bin He},
keywords = {Artificial intelligence, Food safety, Machine learning, Contamination detection, Traceability, Risk assessment},
abstract = {Background
The maintenance of food safety standards is crucial for protecting public health and the economic integrity of the food industry. Artificial Intelligence (AI) is a groundbreaking tool with great potential in the field of food safety. However, its practical applications still require further exploration to fully unlock its benefits.
Scope and approach
This review synthesizes the applications of AI in food safety, focusing on microbial detection, contamination prediction, and traceability within the food supply chain. Additionally, the review discusses AI applications in food safety regulation, including risk assessment, predictive modeling, and decision support systems.
Key findings and conclusions
The integration of AI technologies into food safety practices not only enhances traditional methods of risk management but also fosters a more proactive approach to identifying and mitigating potential hazards. By leveraging AI capabilities, the food industry can significantly improve safety outcomes, reduce operational inefficiencies, and bolster consumer trust. The continued evolution of AI in this domain promises to drive innovative solutions, paving the way for smarter, more resilient food safety systems that prioritize prevention and proactive management.}
}
