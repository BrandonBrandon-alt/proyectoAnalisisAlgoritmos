abstract = {This review provides a comprehensive analysis of adaptive learning platforms (ALPs) in education, focusing on their pedagogical foundations, AI-driven implementations, and the challenges and successes of real-world applications. By collecting and analyzing learner data, ALPs dynamically adjust instructional content and pathways to offer personalized learning experiences, enhancing learning outcomes. The paper explores ALP design frameworks, core algorithms, and evaluation metrics, and assesses their performance across diverse educational contexts through case studies. Lastly, the review highlights key challenges ALPs face, such as privacy concerns and faculty support for implementation, and offers insights into future trends in developing adaptive technologies. This review provides educators and researchers with valuable insights into how ALPs can be effectively applied in various educational settings.}
}
@article{FARZANEGAN2025111363,
title = {Integrating artificial intelligence and optimization tools in bed assignment to enhance patient satisfaction under uncertainty},
journal = {Engineering Applications of Artificial Intelligence},
volume = {159},
pages = {111363},
year = {2025},
issn = {0952-1976},
doi = {https://doi.org/10.1016/j.engappai.2025.111363},
url = {https://www.sciencedirect.com/science/article/pii/S095219762501365X},
author = {Fatemeh Farzanegan and Mohammad Mehdi Sepehri and Mohammad Reza Ghatreh Samani and Leila Barati},
keywords = {Patient bed assignment, Length of stay, Artificial intelligence, Machine learning, Optimization, Stochastic programming},
abstract = {Efficient patient bed assignment is critical for optimizing hospital resource management and enhancing care quality. Hospitals encounter challenges in managing bed capacity due to patient surges, limited bed availability, and staffing shortages, which cause delays in care and are intensified by seasonal fluctuations. This study introduces a novel two-phase methodology for hospital bed allocation that tailors artificial intelligence techniques, combining machine learning models with a mathematical optimization framework. In the first phase, several artificial intelligence-driven models estimate patients' length of stay using demographic and clinical attributes. The gradient boosting model outperformed the others, achieving 96.58 % accuracy, 96.57 % F-score, and 0.965 area under the curve. Accurate prediction enables hospitals to allocate beds more effectively, identify patients at high risk of prolonged stays, and ensure that patients receive appropriate care. In the second phase, the predicted length of stay is incorporated into an integer linear programming model to optimize bed assignment, considering factors such as minimizing patient transfers, ensuring medical isolation, and adhering to age-based room assignment policies. To address the uncertainty in the length of stay predictions, chance-constrained stochastic programming is employed, making this study one of the first to integrate length of stay forecasting with robust bed assignments. The method's applicability and effectiveness are demonstrated through a real-world case study at a pediatric hospital in Gorgan City, Iran. The results indicate the proposed model's potential to enhance bed management through no bed shortage, significant improvement in patient transfers, and approximately 20 % reduction in hospitalization delays compared to current practices.}
}
@incollection{DEMIR2026255,
title = {Chapter 14 - Bibliometric analysis in artificial intelligence and transportation: evaluation of research and future perspectives},
editor = {Gholamreza Haseli and Mostafa Hajiaghaei-Keshteli and Sarbast Moslem},
booktitle = {Reliable Decision-Making for Sustainable Transportation},
