pages = {521-553},
year = {2026},
isbn = {978-0-443-34076-5},
doi = {https://doi.org/10.1016/B978-0-443-34076-5.00006-7},
url = {https://www.sciencedirect.com/science/article/pii/B9780443340765000067},
author = {Temima Ajanović and Farooq Sher and Esma Karahmet Farhat and Roquyya Khatoon and Majlinda Daci and Dan Egesa and Emma Pinjic},
keywords = {Artificial intelligence, education for sustainability, qualified teachers, knowledge in education, ChatGPT, robotics, machine learning, human-centered computing},
abstract = {Designing chemical products is now seen as a new paradigm in chemical engineering (CE). For chemical commodities, efforts have been concentrated on developing databases, design methods, and associated software tools for the past 30 years. Computational chemistry, thermodynamics, material science, CE, industrial engineering, electrical engineering, data science, and artificial intelligence (AI) are just some subjects that must be integrated into the design processes. Increasing the variety of products available that computer-aided tools can handle will be necessary, even though creating more adaptable products will always be difficult. Like the existing process simulators, the prototype chemical product simulators need to be further developed using ongoing improvements in mathematical models, algorithms, computing technologies, and transdisciplinary theories. This will facilitate the faster and more efficient creation of better products that are safer, of higher quality, more sustainable, and more ecologically friendly, all of which can be confirmed through focused trials. The American and European curricula are currently the subject of most studies. Final exam questions from two general chemistry courses were fed into the Chatbox ChatGPT, an AI chatbot. The questions offered both closed-ended and open-ended responses. Even though the closed-response questions mainly relied on chemical metaphors, ChatGPT was able to identify the notion accurately. However, compared to the class average of 69%, ChatGPT’s problem-solving success rate was only 44%. Automation, the future of work, and the possible effects of AI on inequality should all be covered in the curriculum. Teachers should begin facilitating active learning activities instead of only lecturing pupils in engineering and chemistry.}
}
@article{MILLEN2025,
title = {Using generative AI for interview simulations to enhance student research skills in biology education},
journal = {Journal of Microbiology & Biology Education},
