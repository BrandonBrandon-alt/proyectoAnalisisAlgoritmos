@article{GOMEZBRAVO2025100677,
title = {The role of artificial intelligence integrating multi-omics in breast cancer},
journal = {Revista de Senología y Patología Mamaria},
volume = {38},
number = {3},
pages = {100677},
year = {2025},
issn = {0214-1582},
doi = {https://doi.org/10.1016/j.senol.2025.100677},
url = {https://www.sciencedirect.com/science/article/pii/S0214158225000131},
