author = {Etinosa Osaro and Nell Karpinski and Sonia Alornyo and Joshua O. Ighalo},
keywords = {Machine learning (ML), Data-driven material discovery, Inverse design, Computational chemistry, Predictive modeling},
abstract = {The landscape of materials science and chemistry has undergone a profound transformation driven by artificial intelligence (AI) and machine learning (ML) methodologies. Traditional empirical approaches, reliant on trial-and-error experimentation and heuristic models, are increasingly supplanted by data-driven paradigms that leverage vast computational resources and sophisticated algorithms. This review examines the evolution from empirical foundations to intelligent computational frameworks, analyzes current state-of-the-art AI applications across materials discovery and chemical synthesis, and projects future trajectories in this rapidly evolving field. We discuss how AI has enabled unprecedented capabilities in property prediction, inverse materials design, autonomous experimentation, and structural characterization while addressing persistent challenges in data quality, model interpretability, and interdisciplinary integration. The convergence of AI with quantum mechanical simulations, high-throughput experimentation, and autonomous laboratories heralds a new era of accelerated scientific discovery that promises to address critical global challenges in energy, sustainability, and advanced materials.}
}
@article{SUTANTO2025107741,
