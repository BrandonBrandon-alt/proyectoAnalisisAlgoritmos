url = {https://www.sciencedirect.com/science/article/pii/S259023852500181X},
author = {Longhai Zhang and Qiming Bing and Huang Qin and Liang Yu and Haobo Li and Dehui Deng},
keywords = {artificial intelligence, catalyst material synthesis, machine learning, catalyst design, synthesis condition optimization, high-throughput experimentation, automated synthesis workflow},
abstract = {Summary
Accurate and efficient synthesis of catalysts is a critical issue in catalysis research. Recent advancements in artificial intelligence (AI) provide a transformative opportunity for catalyst synthesis, shifting from traditional experience-driven approaches to data-driven, automated, and intelligent design and synthesis methodology. This perspective highlights representative progress in AI-assisted catalyst synthesis, emphasizing the machine learning (ML) methods that predict catalyst structure and performance, optimize synthesis conditions, and drive automated high-throughput experimentation and characterization. In the vision of promoting deeper integration of AI into catalyst synthesis and achieving a closed-loop workflow, feasible routes of applying ML for promoting descriptor identification, large-scale calculation, and catalytic activity and stability prediction are specifically reviewed. The application of cutting-edge ML methods, such as the active learning and generative model, in catalyst design and synthesis is also discussed. The new paradigm of AI-driven catalyst synthesis is promising to benefit discovery efficiency in catalysis, material, and energy fields.}
}
@incollection{MITCHELL202595,
title = {Chapter 6 - Artificial intelligence in pathology: Easing the burden of annotation},
editor = {Chhavi Chauhan and Stanley Cohen},
booktitle = {Artificial Intelligence in Pathology (Second Edition)},
publisher = {Elsevier},
edition = {Second Edition},
pages = {95-108},
year = {2025},
isbn = {978-0-323-95359-7},
doi = {https://doi.org/10.1016/B978-0-323-95359-7.00006-6},
url = {https://www.sciencedirect.com/science/article/pii/B9780323953597000066},
author = {Benjamin R. Mitchell and Marion C. Cohen and Stanley Cohen},
keywords = {Transfer learning, Zero-shot earning, Siamese netwroks, One-glass models, Generative networks, Reinforcement learning},
abstract = {It has become clear that the need for huge datasets represents a bottleneck for applications of artificial intelligence classes. In pathology, there is the additional problem that there are often substantially fewer annotated target lesions than normal tissues for comparison. While deep learning algorithms have successfully tackled problems previously thought to be solely the domain of humans, this dependence on massive amounts of data is one of the major ways in which computer models of the brain are deficient. Organic brains utilize large numbers of specialized neural nets arranged in both linear and parallel fashion, with each solving a restricted classification problem. They rely on local error corrections such as Hebbian learning (“neurons that fire together wire together”) as compared to the nonlocal back-propagation used in most implementations of artificial neural nets. They incorporate reinforcement into their learning strategy. For these reasons, even toddlers can easily classify objects after one or only a few examples. Rather than provide an overview of current research articles on artificial intelligence in the pathology literature, this review will focus on general strategies for dealing with the huge datasets that we routinely encounter. These include transfer learning, zero-shot learning, Siamese networks, one-class models, generative networks, and reinforcement learning. Neither an extensive mathematical background nor advanced programming skills are needed in order to make these subjects accessible to pathologists with limited background in the nuts and bolts of machine learning. However, some familiarity with the basic principles of deep learning is necessary. In chapter 4 we provide a detailed description of these approaches. In this chapter we show how they are used to ameliorate training data needs. By providing an overview and extension of these concepts, this material will help to understand some of the current limitations of machine learning and how to address these. We begin with classical approaches and then move on to newer methodologies.}
}
@article{LUO20251155,
