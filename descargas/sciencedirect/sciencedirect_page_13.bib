author = {BALJASH CHEEMA and JONATHAN HOURMOZDI and ADRIENNE KLINE and FARAZ AHMAD and ROHAN KHERA},
keywords = {Artificial intelligence, machine learning, heart failure},
abstract = {ABSTRACT
Artificial intelligence (AI) has the potential to revolutionize the management of heart failure. AI-based tools can guide the diagnosis and treatment of known risk factors, identify asymptomatic structural heart disease, improve cardiomyopathy diagnosis and symptomatic heart failure treatment, and uncover patients transitioning to advanced disease. By integrating multimodal data, including omics, imaging, signals, and electronic health records, state-of-the-art algorithms allow for a more tailored approach to patient care, addressing the unique needs of the individual. The past decade has led to the development of numerous AI solutions targeting each aspect of the heart failure syndrome. However, significant barriers to implementation remain and have limited clinical uptake. Data-privacy concerns, real-world model performance, integration challenges, trust in AI, model governance, and concerns about fairness and bias are some of the topics requiring additional research and the development of best practices. This review highlights progress in the use of AI to guide the diagnosis and management of heart failure while underscoring the importance of overcoming key implementation challenges that are currently slowing progress.}
}
@article{MIR2025111413,
title = {Advancements in deep learning and explainable artificial intelligence for enhanced medical image analysis: A comprehensive survey and future directions},
journal = {Engineering Applications of Artificial Intelligence},
volume = {158},
pages = {111413},
year = {2025},
issn = {0952-1976},
doi = {https://doi.org/10.1016/j.engappai.2025.111413},
url = {https://www.sciencedirect.com/science/article/pii/S0952197625014150},
author = {Aqib Nazir Mir and Danish Raza Rizvi},
keywords = {Textual explanations, Interpretability, Explainability, Deep learning, Medical imaging},
abstract = {With the notable advancements in deep learning, the demand for explainability and interpretability has witnessed a substantial surge, particularly within the medical domain, where critical decisions are at stake. The concept of Explainable Artificial Intelligence (XAI) has emerged as a burgeoning field focused on elucidating the opaque nature of deep learning systems by evaluating these models and providing insights into their decision-making processes. Due to their inherent lack of transparency, deep learning models often fall short of explicitly articulating the rationale behind their predictions. Consequently, integrating most deep learning algorithms into real-world clinical decision-making workflows remains limited. Our review paper presents a comprehensive analysis of XAI techniques in the context of medical imaging analysis. Through a systematic review of existing literature, we offer insights into the comparative landscape of XAI approaches, meticulously evaluating prominent medical imaging datasets and identifying gaps in prior research. We provide a concise overview of state-of-the-art interpretable models and post-hoc methods, accompanied by an assessment of suitable evaluation metrics for benchmarking these approaches. Additionally, we introduce a taxonomy for categorizing XAI methods, encompassing visual, textual, example-based, and concept-based approaches. Our discussion extends to future research directions and challenges, aiming to guide the development of innovative solutions in this rapidly evolving field.}
}
