@article{HOLT2025102567,
title = {Role of Artificial Intelligence in Congenital Heart Disease and Interventions},
journal = {Journal of the Society for Cardiovascular Angiography & Interventions},
volume = {4},
number = {3, Part B},
pages = {102567},
year = {2025},
