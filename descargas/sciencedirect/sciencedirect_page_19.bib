@article{GUERRA2023e160,
title = {GPT-4 Artificial Intelligence Model Outperforms ChatGPT, Medical Students, and Neurosurgery Residents on Neurosurgery Written Board-Like Questions},
journal = {World Neurosurgery},
volume = {179},
pages = {e160-e165},
year = {2023},
issn = {1878-8750},
doi = {https://doi.org/10.1016/j.wneu.2023.08.042},
url = {https://www.sciencedirect.com/science/article/pii/S1878875023011440},
author = {Gage A. Guerra and Hayden Hofmann and Sina Sobhani and Grady Hofmann and David Gomez and Daniel Soroudi and Benjamin S. Hopkins and Jonathan Dallas and Dhiraj J. Pangal and Stephanie Cheok and Vincent N. Nguyen and William J. Mack and Gabriel Zada},
keywords = {Artificial intelligence, ChatGPT, GPT-4, Machine learning, Neurosurgical boards, Neurosurgical training, SANS question bank},
abstract = {Background
Artificial intelligence (AI) and machine learning have transformed health care with applications in various specialized fields. Neurosurgery can benefit from artificial intelligence in surgical planning, predicting patient outcomes, and analyzing neuroimaging data. GPT-4, an updated language model with additional training parameters, has exhibited exceptional performance on standardized exams. This study examines GPT-4’s competence on neurosurgical board-style questions, comparing its performance with medical students and residents, to explore its potential in medical education and clinical decision-making.
Methods
GPT-4’s performance was examined on 643 Congress of Neurological Surgeons Self-Assessment Neurosurgery Exam (SANS) board-style questions from various neurosurgery subspecialties. Of these, 477 were text-based and 166 contained images. GPT-4 refused to answer 52 questions that contained no text. The remaining 591 questions were inputted into GPT-4, and its performance was evaluated based on first-time responses. Raw scores were analyzed across subspecialties and question types, and then compared to previous findings on Chat Generative pre-trained transformer performance against SANS users, medical students, and neurosurgery residents.
Results
GPT-4 attempted 91.9% of Congress of Neurological Surgeons SANS questions and achieved 76.6% accuracy. The model’s accuracy increased to 79.0% for text-only questions. GPT-4 outperformed Chat Generative pre-trained transformer (P < 0.001) and scored highest in pain/peripheral nerve (84%) and lowest in spine (73%) categories. It exceeded the performance of medical students (26.3%), neurosurgery residents (61.5%), and the national average of SANS users (69.3%) across all categories.
Conclusions
GPT-4 significantly outperformed medical students, neurosurgery residents, and the national average of SANS users. The mode’s accuracy suggests potential applications in educational settings and clinical decision-making, enhancing provider efficiency, and improving patient care.}
}
@article{ZHANG2025105138,
title = {Artificial intelligence boosting multi-dimensional information fusion: Data collection, processing and modeling for food quality and safety assessment},
journal = {Trends in Food Science & Technology},
volume = {163},
pages = {105138},
year = {2025},
issn = {0924-2244},
doi = {https://doi.org/10.1016/j.tifs.2025.105138},
url = {https://www.sciencedirect.com/science/article/pii/S0924224425002742},
author = {Hengrui Zhang and Yuandong Lin and Shilin Cao and Jun-Hu Cheng and Xin-An Zeng},
keywords = {Multi-dimensional data fusion, Machine learning, Food quality and safety, Deep learning, Intelligent monitoring},
abstract = {Background
Significant challenges are posed to food quality and safety assurance by the growing complexity of global food supply chains and the increasing demand for efficient non-destructive assessment methods. These challenges are addressed through emerging multidimensional data fusion technology. High-dimensional data processing efficiency has been substantially improved by artificial intelligence-driven machine learning (ML) and deep learning (DL) methodologies. Comprehensive, rapid, and non-destructive monitoring of food quality is enabled through the integration of multimodal data sources using these methods.
Scope and approach
Recent advances in food quality and safety inspection through multi-dimensional data fusion techniques are systematically summarized in this review. The entire workflow, encompassing data acquisition, processing, and modeling, is comprehensively examined. The state-of-the-art acquisition methods for one-dimensional data (e.g., sound and gas sensors), two-dimensional data (e.g., visual imaging), and three-dimensional data (e.g., hyperspectral imaging and computed tomography) are introduced. Common data processing techniques and feature extraction methods across various data types and dimensions are discussed, with an emphasis on their role in the integration of heterogeneous data. Additionally, the application of ML and DL methods in multi-dimensional data fusion is reported, with a focus on their impact on enhancing the accuracy, robustness, and scalability of food quality monitoring systems. Representative applications, such as food quality testing, pollution monitoring, and compositional analysis, are also examined. Finally, the challenges and future directions of multi-dimensional data fusion techniques in food quality assessment are explored.
Key findings and conclusions
Detection techniques based on multimodal data fusion exhibit superior potential compared to single-modal approaches for food quality assessment, driven by advanced ML and DL methods. Through the integration of data from multiple sensors, an efficient, stable, and accurate solution is offered for non-destructive food quality monitoring, aligning with the growing demands of food industry 5.0. Owing to continuous advancements in artificial intelligence technology, inspection solutions utilizing multidimensional data fusion are expected to emerge as the dominant approach for intelligent food quality assessment, providing enhanced accuracy and efficiency for complex supply chains.}
}
