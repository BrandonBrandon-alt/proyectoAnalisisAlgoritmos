abstract = {Effective management of diabetes requires a multifaceted/multidimensional approach, including lifestyle modifications, regular blood glucose monitoring, comprehensive screening for diabetes-related complications, and interdisciplinary collaboration involving various medical fields such as endocrinology, podiatry, nutrition, nephrology, neurology, and ophthalmology. However, limited access to advanced medical resources and inadequate primary healthcare facilities (especially in remote areas) ultimately leads to inferior/poor management of blood glucose levels among diabetes patients. Therefore, the integration of technology into diabetes care is of utmost importance to assist healthcare professionals in managing diabetes. Over the past decade, artificial intelligence/machine learning has revolutionized healthcare by opening new diagnostic and therapeutic horizons for managing and treating a wide variety of medical conditions. In recent years, artificial intelligence (AI) has also been widely applied in the field of diabetes, leading to numerous advances in disease prediction, diagnosis, complication screening, management, and treatment. This chapter gives an overview of artificial intelligence in the management of diabetes, AI/Machine Learning (ML) techniques, and their applications and limitations in diabetes management. FDA-approved AI/ML-powered medical devices in diabetes management are briefly reviewed. In addition, regulatory requirements for AI/ML-enabled medical devices across Europe, the United States, and China are also discussed.}
}
@article{QIAO2025,
title = {The Convergence of Artificial Intelligence and Microfluidics in Drug Research and Development},
journal = {Engineering},
year = {2025},
issn = {2095-8099},
doi = {https://doi.org/10.1016/j.eng.2025.07.025},
url = {https://www.sciencedirect.com/science/article/pii/S2095809925004357},
author = {Du Qiao and Hongxia Li and Xue Zhang and Xuhui Chen and Jiang Zhang and Jianan Zou and Danyang Zhao and Weiping Zhu and Xuhong Qian and Honglin Li},
keywords = {Artificial intelligence, Machine learning, Deep learning, Microfluidics, Drug discovery, Drug evaluation, Drug manufacturing, Drug delivery},
abstract = {Drug research and development (R&D) plays a crucial role in supporting public health. However, the traditional drug-discovery paradigm is hindered by significant drawbacks, including high costs, lengthy development timelines, high failure rates, and limited output of new drugs. Recent advances in micro/nanotechnology, along with progress in computer science, have positioned microfluidics and artificial intelligence (AI) as promising transformative tools for drug development. Microfluidics offers miniaturized, multiplexed, and versatile platforms for high-dimensional data acquisition, while AI enables the rapid processing of complex, large-scale microfluidic data; together, they are accelerating a paradigm shift in the drug-discovery process. This paper first outlines the mainstream microfluidic strategies and AI models used in drug R&D. It then summarizes and discusses real-world applications of the integrated use of these technologies across various stages of drug discovery, including early drug discovery, drug screening, drug evaluation, drug manufacturing, and drug delivery systems. Finally, the paper examines the main limitations of microfluidics and AI in drug R&D and offers an outlook on the future convergence of these technologies.}
}
@article{NIMMY2025113333,
title = {Explainable Artificial Intelligence (XAI) in glaucoma assessment: Advancing the frontiers of machine learning algorithms},
