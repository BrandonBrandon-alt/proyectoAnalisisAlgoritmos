keywords = {Confocal microscopy, Endothelium, Residual self-attention mechanism, Spatial dependencies, Dilated convolutional modules, Corneal images, Diseased corneas},
abstract = {Confocal microscopy enables in-depth visualization of each layer within the human cornea. The endothelium, epithelium, and stroma are critical layers susceptible to various illnesses, underscoring the need for accurate layer classification to generate automated medical insights. Unlike conventional methods, Artificial Intelligence approaches enhance accuracy and reduce the time required for manual evaluation by clinical professionals. This paper presents innovative feature extraction and classification approaches using the Residual Self-Attention Hierarchical Fusion model and the Dilated Convolutional DenseNet-77 model. The Residual Self-Attention Hierarchical Fusion model combines Hierarchical Fusion with a Residual Self-Attention mechanism to capture complete spatial dependencies and contextual data. Additionally, the Dilated Convolutional DenseNet-77 model integrates dilated convolutional modules to improve the classification of confocal corneal images. This advanced integration effectively manages the variability and complexity of corneal datasets, leading to increased accuracy and interpretability in distinguishing between healthy and diseased corneas. The system has been evaluated on four datasets, and the proposed Dilated Convolutional DenseNet-77 model achieves an accuracy, precision and recall of 98.5%, 98.2% and 98.3% respectively based on the Keratoconus detection dataset. These capabilities support comprehensive clinical analysis, evaluation, and diagnosis of corneal conditions.}
}
@article{SAHOO20251314,
title = {Artificial Intelligence in Cancer Diagnosis: A Game-changer in Healthcare},
