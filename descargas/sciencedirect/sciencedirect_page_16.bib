keywords = {Natural products, Artificial intelligence, Anti-aging pharmacology, Green extraction, Microphysiological systems, Digital twins},
abstract = {Aging is increasingly recognized as a process driven by oxidative stress, chronic inflammation, mitochondrial decline, and impaired autophagy. Natural products provide a rich source of antioxidants and autophagy modulators that can counteract these hallmarks of aging, yet their translation into effective interventions has been slow. Recent advances in artificial intelligence (AI), omics and bioinformatic technologies now enable the systematic identification of active compounds, prediction of their molecular targets, and optimization of synergistic combinations. Within a systems pharmacology framework, these tools can transform fragmented findings into an integrated understanding of how natural products promote healthy aging. This perspective highlights the urgent need for human-centered strategies that combine traditional pharmacology with AI-driven discovery to advance safe, effective, and personalized anti-aging preventions/therapeutics.}
}
@article{LI2024113152,
